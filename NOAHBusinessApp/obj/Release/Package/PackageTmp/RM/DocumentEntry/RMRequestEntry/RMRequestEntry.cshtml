@inherits System.Web.Mvc.WebViewPage			
@{			
ViewBag.Title = "Request Entry";			
Layout = NoahWebLib.nwSystem.GetDefaultLayout();			
}			
<head>
    <script src="~/RM/DocumentEntry/RMRequestEntry/RMRequestEntry.js"></script>
    <link href="~/RM/DocumentEntry/RMRequestEntry/RMRequestEntry.css" rel="stylesheet" />
</head>						
<body>
    <div id="noah-webui-Toolbox" class="nk-toolbar-c noah-webui-Toolbox">
        <div class="nk-toolbar-btn noah-webui-Toolbox">
            <div id="noah-webui-default-New" class="btn-tb-action btn-tb-new noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">New</div>
            </div>
            <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-save noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Save</div>
            </div>
            <div id="noah-webui-default-Delete" class="btn-tb-action btn-tb-delete noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Delete</div>
            </div>
            <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Refresh</div>
            </div>
            <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Inquire</div>
            </div>
            <div id="noah-webui-default-Process" class="btn-tb-action btn-tb-process noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Process</div>
            </div>
            <div id="noah-webui-default-Print" class="btn-tb-action btn-tb-print noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Print</div>
            </div>
            <div id="noah-webui-default-Export" class="btn-tb-action btn-tb-export noah-webui-Toolbox-Item nwHide">
                <div class="_icon"></div>
                <div class="_lbl">Export</div>
            </div>
            @*<div id="btnVwBdgtMonitoring" class="btn-tb-action btn-tb-VwBdgtMonitoring noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">View Budget Availability Report</div>
            </div>*@
            <div id="btnVwBdgtMonitoring" class="btn-tb-action btn-tb-closing">
                <div class="_icon"></div>
                <div class="_lbl">View Budget Availability Report</div>
            </div>
            <div id="noah-webui-default-btnLoadFrmValidHist" class="btn-tb-action btn-tb-search btnLoadFrmValidHist noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Load from Historical Template</div>
            </div>
            @*<div id="noah-webui-default-CreateSBR" class="btn-tb-action noah-webui-Toolbox-Item btnCreateSBR">*@
             <div id="noah-webui-default-btnCreateSBR" class="btn-tb-action btnCreateSBR btn-tb-import noah-webui-Toolbox-Item">
                 <div class="_icon"></div>
                 <div class="_lbl">Create SBR</div>
             </div>
        </div>
    </div>

    <div class="container-flex padding-10">
        <div class="row row-parent">
            <div class="col col-6 col-parent">
                <input id="isAllwBackdateTran" class="nwHide" />
                <input id="txtServerdate" class="nwHide" />
                <input id="txtTagPC" class="nwHide" />
                <input id="txtTagCC" class="nwHide" />
                <input id="txtDeptDesc" class="nwHide" />
                <input id="txtEnableChargingSave" class="nwHide" />
                <input id="txtRealignmentLink" class="nwHide" />
                <input id="txtRealignmentLink" class="nwHide" />


                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Request Objective</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtTrantype" type="text" class="txtbox nwHide" />
                        <input id="txtReqObjectiveCode" type="text" class="txtbox nwHide" />
                        <input id="txtReqObjective" type="text"  class="txtbox"/>
                    </div>
                </div>
                <div class="row with-label-lookup">
                    <div class="col col-4">
                        <label class="lbltxt">Location with Accountable Forms<span class="nwRequiredField">*</span></label>
                    </div>
                    <div class="col col-8 col-parent lookups" id="lugLocForm">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>
                <div class="row with-label-lookup">
                    <div class="col col-4">
                        <label class="lbltxt">Requestor/Custodian</label>
                    </div>
                    <div id="lugCreatorCustodian" class="col col-8 col-parent lookups">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Requestor/Custodian Contact Details</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtContactNo" type="text" name="" class="txtbox" maxlength="30" >
                    </div>
                </div>

                <div class="row with-label-lookup-single">
                    <div class="col col-4">
                        <label class="lblCostCenter" id="txtRequiredCostCenter">Cost Center<span class="nwRequiredField">*</span></label>
                        <label class="lblCostCenter nwHide" id="txtLoadedCostCenter">Cost Center</label>
                    </div>
                    <div id="lugCostCenter" class="col col-8 col-parent lookups">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval txtlookup">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval txtlookup">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>

                <div class="row with-label-lookup-single">
                    <div class="col col-4">
                        <label class="lbltxt">Currency<span class="nwRequiredField">*</span></label>
                    </div>
                    <div id="lugCurrency" class="col col-8 col-parent lookups">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval txtlookup">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval txtlookup">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>
                <div class="row with-label-lookup-single nwHide" id="consumption_show">
                    <div class="col col-4">
                        <label class="lbltxt">Reason for Request<span class="nwRequiredField">*</span></label>
                    </div>
                    <div id="lugRsnForRequest" class="col col-8 col-parent lookups">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval txtlookup">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval txtlookup">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                        <div class="nwHide">
                            <input type="text" id="txtReasonType" hidden />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Particulars<span class="nwRequiredField">*</span></label>
                    </div>
                    <div class="col col-8">
                        <textarea id="txtParticular" class="txtParticular autoresize" rows="3" maxlength="500"></textarea>
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <div id="btnReqCompliance" class="btn btn-default btn-default-orange btnMedium fullwidth">
                            Requirements Compliance
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <input type="text" id="txtHasChanges" class="nwHide" />
                    </div>
                </div>
                <div class="row nwHide">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <input id="txtIsUpperCaseParticulars" type="text"/>
                        </div>
                </div>
                <div class="row nwHide">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <input id="txtVatRate" type="text" />
                    </div>
                </div>
            </div>
            <div class="col col-6 col-parent">
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Transaction No.</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtDocno" type="text" name="" class="txtbox">
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Date Submitted</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtValueDate" type="text" name="" class="txtbox nwHide">
                        <input id="txtDateSubmitted" type="text" name="" class="txtbox">
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Date Posted</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtDatePosted" type="text" name="" class="txtbox">
                        <input id="txtDatePosted2" type="text" name="" class="txtbox nwHide">
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Document Status</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtDocStatus" type="text" name="" class="txtbox">
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Reason for Disapproval</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtReason" type="text" name="" class="txtbox">
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Disapproval Remarks</label>
                    </div>
                    <div class="col col-8">
                        <textarea class="autoresize" rows="3" id="txtDisapprovalRemarks"></textarea>
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Total Request Amount (VATEX)</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtTotReqAmntVatex" type="text" name="" class="txtbox isNumber" maxlength="20">
                    </div>
                </div>
                <div class="row nwHide">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <input id="txtValueDate2" type="text" class="nwDatePick" />
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Total Request Amount (VATIN)</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtTotReqAmnt" type="text" name="" class="txtbox isNumber" maxlength="20">
                        <input id="txtNOAccessReqrep" type="text" name="" class="txtbox isNumber nwHide" maxlength="80">
                    </div>
                </div>
                <div class="row" id="isAllowBackDateWrapper">
                    <div class="col col-4">
                        <label class="lbltxt">Value Date</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtValueDate2" type="text" class="nwDatePick"/>
                    </div>
                </div>
            </div>
        </div>

        <div class="row row-parent" id="CatalogueGrid">
            <div class="col col-12 col-parent">
                <div class="row">
                    <div id="nwGridCon" class="fullwidth">
                    </div>
                </div>
            </div>
        </div>

        <div class="row row-parent" id="NonCatalogueGrid">
            <div class="col col-12 col-parent">
                <div class="row">
                    <div id="nwGridConNonCatalogue" class="fullwidth">
                    </div>
                </div>
            </div>
        </div>

        <div class="nk-footer-c">
            <div class="nk-footer-info">
                <div class="nk-footer-create">
                    <div class="nkf nk-f-createBy">
                        <svg xmlns="http://www.w3.org/2000/svg" width="22px" height="22px" class="svg-docFooter-icon" data-name="Layer 1" viewBox="0 0 22 22">
                            <path class="svg-docFooter-icon-a" d="M13,2H5A3,3,0,0,0,2,5V17a3,3,0,0,0,3,3h8a3,3,0,0,0,3-3V5A3,3,0,0,0,13,2Zm2,15a2,2,0,0,1-2,2H5a2,2,0,0,1-2-2V5A2,2,0,0,1,5,3h8a2,2,0,0,1,2,2Zm2.5,0h0a.5.5,0,0,1-.5-.5V5.5a.5.5,0,0,1,.5-.5h0a.5.5,0,0,1,.5.5v11A.5.5,0,0,1,17.5,17Zm2-1h0a.5.5,0,0,1-.5-.5v-9a.5.5,0,0,1,.5-.5h0a.5.5,0,0,1,.5.5v9A.5.5,0,0,1,19.5,16ZM13,8H5V7h8Zm-2,3H5V10h6ZM9,14H5V13H9Z">
                            </path>
                        </svg>
                        <div id="nwlbl_RecUser" class="nkf-txt">Created By:</div>
                    </div>
                    <div id="nwtxt_RecUser" class="nkf nk-f-createName"></div>
                    <div id="nwlbl_RecDate" class="nkf nk-footer-x"></div>
                    <div id="nwtxt_RecDate" class="nkf nk-f-createDate" value=""></div>
                </div>
                <div class="nk-footer-x"></div>
                <div class="nk-footer-modified">
                    <div class="nkf nk-f-modifiedBy">
                        <svg xmlns="http://www.w3.org/2000/svg" width="22px" height="22px" class="svg-docFooter-icon" data-name="Layer 1" viewBox="0 0 22 22">
                            <path class="svg-docFooter-icon-a" d="M13,2H5A3,3,0,0,0,2,5V17a3,3,0,0,0,3,3h8a3,3,0,0,0,3-3V5A3,3,0,0,0,13,2Zm2,15a2,2,0,0,1-2,2H5a2,2,0,0,1-2-2V5A2,2,0,0,1,5,3h8a2,2,0,0,1,2,2Zm2.5,0h0a.5.5,0,0,1-.5-.5V5.5a.5.5,0,0,1,.5-.5h0a.5.5,0,0,1,.5.5v11A.5.5,0,0,1,17.5,17Zm2-1h0a.5.5,0,0,1-.5-.5v-9a.5.5,0,0,1,.5-.5h0a.5.5,0,0,1,.5.5v9A.5.5,0,0,1,19.5,16ZM13,8H5V7h8Zm-2,3H5V10h6ZM9,14H5V13H9Z">
                            </path>
                        </svg>
                        <div id="nwlbl_ModUser" class="nkf-txt">Modified By:</div>
                    </div>
                    <div id="nwtxt_ModUser" class="nkf nk-f-modifiedName"></div>
                    <div id="nwlbl_ModDate" class="nkf nk-footer-x"></div>
                    <div id="nwtxt_ModDate" class="nkf nk-f-modifiedDate" value=""></div>
                </div>
            </div>
        </div>
   </div>



    @section noahpopup{
    @RenderPage("~/RM/DocumentEntry/RMRequestEntry/Popup/PopupWindows.cshtml", "ValidationPopup")
    }
</body>
