
@{
    ViewBag.Title = "Uploading";
    string Upload_Title = "Upload File";
    string MenuCode = Page[0].ToString();
}
<script src="~/materials/jslib/uploading.js"></script>

<div id="nwUploadCon" nwTitle="Upload File">
        <div class="row-parent">
            <div class="col col-12">

                @using (Html.BeginForm("Upload", MenuCode))
            {
                    @Html.AntiForgeryToken()
                    <div class="row margin-top-10">
                        <div class="col col-10">
                            <input type="hidden" name="Practice" value="DefaultText"><br>
                            <input id="myfile" type="file" accept="" name="myfile" />

                        </div>
                    </div>
                    <div class="row margin-top-10">
                        <div class="col col-10">
                            <input type="submit" style="display: block" value="Upload" id="btnupload" class="btn btn-default" />
                        </div>
                    </div>
                }
                @*}*@
                <div class="row">
                    <div class="col col-10">
                        <div id="prgbarUpload" class="progressbar progressbar-l progressbar-striped progressbar-animated">
                            <div class="progressbar-line bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 0%;"><span class="percent">0%</span></div>
                        </div>
                    </div>
                </div>
                <div class="row margin-top-10">
                    <div class="col col-10">
                        <div id="status"></div>
                    </div>
                </div>
                <div class="row margin-top-10">
                    <div class="col col-10">
                        <i>
                            Notes:<br />
                            <b><i>Note:</i></b> Acceptable file size is 50mb only
                        </i>
                    </div>
                </div>
            </div>
        </div>
</div>

