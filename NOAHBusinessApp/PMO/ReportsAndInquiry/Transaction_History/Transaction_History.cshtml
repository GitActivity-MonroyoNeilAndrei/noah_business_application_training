@inherits System.Web.Mvc.WebViewPage
@{
     ViewBag.Title = "Transaction History";
     Layout =NoahWebLib.nwSystem.GetDefaultLayout();
}

<head>
    <script src="~/RE/ReportsAndInquiry/Transaction_History/Transaction_History.js"></script>
    <link href="~/RE/ReportsAndInquiry/Transaction_History/Transaction_History.css" rel="stylesheet" />
</head>

<div id="noah-webui-Toolbox" class="nk-toolbar-c">
    <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh">
        <div class="_icon"></div>
        <div class="_lbl">Refresh</div>
    </div>
</div>

<div class="container-flex">
    <div class="row row-parent padding-10">
        <div class="col col-4 col-parent">
            <!-- 1ST Field Set -->
            <fieldset class="fieldset" nk-fieldset-style="6">
                <!-- Account No. -->
                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Account No.</label>
                    </div>
                    <div class="col col-7">
                        <select id="dpAccNo" class="cmb fullwidth">
                        </select>
                    </div>
                </div>
                <!-- Unit No. -->
                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Unit No.</label>
                    </div>
                    <div class="col col-7">
                        <input id="txtUnitNo" type="text" name="" class="txtbox">
                    </div>
                </div>
            </fieldset>
            <!-- Space -->
            <div class="row">
                <div class="col col-12">
                    <label class="lbltxt"></label>
                </div>
            </div>
        </div>
        <div class="col col-4 col-parent">
            <div class="col col-12 padding-10">
                <!-- Date From -->
                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Date From</label>
                    </div>
                    <div class="col col-7">
                        <input id="txtFrom" type="text" name="" class="txtbox inputFilter txtDate txtbox-date nwDatePick">
                    </div>
                </div>
                <!-- Date To -->
                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Date To</label>
                    </div>
                    <div class="col col-7">
                        <input id="txtTo" type="text" name="" class="txtbox inputFilter txtDate txtbox-date nwDatePick">
                    </div>
                </div>
            </div>
            <!-- Space -->
            <div class="row">
                <div class="col col-12">
                    <label class="lbltxt"></label>
                </div>
            </div>
        </div>
        <div class="col col-4 col-parent">
            <div class="col col-12 padding-10">
                <!-- Transaction Type -->
                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Transaction Type</label>
                    </div>
                    <div class="col col-7">
                        <select id="dpTranType" class="cmb fullwidth">
                        </select>
                    </div>
                </div>
                <!-- Request -->
                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Request</label>
                    </div>
                    <div class="col col-7">
                        <select id="dpRequest" class="cmb fullwidth">
                        </select>
                    </div>
                </div>
            </div>
            <!-- Space -->
            <div class="row">
                <div class="col col-12">
                    <label class="lbltxt"></label>
                </div>
            </div>
        </div>
    </div>
    <div class="row row-parent padding-10">
        <div id="TranConGrid" class="col col-12">
        </div>
    </div>
</div>