@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "P8-FORW";
    Layout = "~/forms_standards/StandardLayout/NOAHSpreadDesigner/NOAHSpreadDesigner.cshtml";
}
<head>
    <script src="~/P8/ReportsAndInquiry/P8FORWViewer/P8FORWViewer.js"></script>
    <link href="~/P8/ReportsAndInquiry/P8FORWViewer/P8FORWViewer.css" rel="stylesheet" />
</head>



@*hidden*@
<input id="txt-P8FORW-code" type="hidden" name="" class="txtbox txtcode" autocomplete="off">
<div id="txt-P8FORWViewer-data" type="hidden"></div>


<div class="nwe-flex-c" id="p8forwviewer-Toolbox">
    <div class="nwe-g-main">
        <div class="nwe-g-btn">
            <div class="nwe-g-c">
                <div id="p8forwviewer-btnClear" class="nwe-cl-c" title="Clear">
                    <div class="nwe-cl-icon-m cviewer"></div>
                    <div class="nwe-st-icontext">Clear</div>
                </div>
                <div id="p8forwviewer-btnRefresh" class="nwe-cl-c" title="Refresh">
                    <div class="nwe-cl-icon-m refreshviewer"></div>
                    <div class="nwe-st-icontext">Refresh</div>
                </div>
                <div id="p8forwviewer-btnExport" class="nwe-cl-c" title="Export">
                    <div class="nwe-cl-icon-m exportviewer"></div>
                    <div class="nwe-st-icontext">Export</div>
                </div>
            </div>
        </div>
        <div class="nwe-title">Toolbox</div>
    </div>
</div>
<div class="nwe-flex-c" id="p8forwviewer-Formula">
    <div class="nwe-g-main">
        <div class="nwe-g-btn">
            <div class="nwe-g-c">
                <div id="faFormulas" class="nwe-cl-c" title="Show Formulas">
                    <div class="nwe-cl-icon-m shf"></div>
                    <div class="nwe-st-icontext">Show Formulas</div>
                </div>
            </div>
        </div>
        <div class="nwe-title">Functions</div>
    </div>
</div>