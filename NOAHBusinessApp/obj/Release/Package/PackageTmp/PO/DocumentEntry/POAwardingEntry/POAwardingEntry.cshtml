                    <div class="col col-3">
                        <label class="lbltxt">Date Posted</label>
                    </div>
                    <div class="col col-5">
                        <input type="text" name="" class="txtbox" id="dtpDatePosted"/>
                    </div>
                    <div class="col col-4">
                    </div>
                </div>

                @*Document Status*@
