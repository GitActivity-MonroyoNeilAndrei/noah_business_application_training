                                                </div>
                                                <div class="col col-1-75">
                                                    <label class="lbltxt">Street Name<span class="streetname nwOptionalField">*</span></label>
                                                </div>
                                                <div class="col col-3">
                                                    <input id="txtStreetName" type="text" maxlength="80" />
                                                </div>
                                            </div>


                                            @*Lot, Block, Phase*@
                                            <div class="row">
