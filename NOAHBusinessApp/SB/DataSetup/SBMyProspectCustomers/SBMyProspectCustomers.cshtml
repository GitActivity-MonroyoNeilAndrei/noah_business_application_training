                        <label id="lblchkWithClientApptSlip" class="lbl-chk" for="chkWithClientApptSlip">With Client Appointment Slip </label>
                    </div>
                </div>
            </div>


            </div>
        <div class="col col-3 col-parent">

            <div>
                <button data-target="" id="btnSLPC" class="btn btn-default nwcuzCustom-002" disabled>Send Link to Prospect Customers</button>
            </div>
            <div class="row">
                <fieldset id="profile-box">
                    <legend>Picture</legend>
                    <div class="noah-webui-containerRowItem">
                        <div>
                            <div id="profile-img"></div>
                        </div>
                        <div>
                            <button data-target="" id="attachIDPicture" class="data-entry btn-default" disabled>Attach ID Picture</button>
                        </div>
                    </div>
                </fieldset>
            </div>


            <div class="row">
                <fieldset id="signature-box">
                    <legend>Signature</legend>
                    <div class="noah-webui-containerRowItem">
                        <div>
                            <div id="profile-img-signature"></div>
                        </div>
                        <div>
                            <button data-target="" id="attachSignature" class="data-entry btn-default" disabled>Attach Signature</button>
                        </div>
                    </div>
                </fieldset>

            </div>


            <div class="row">
                <button class="btn btn-default btn-default-orange nwcuzCustom-002" id="btnReqCompliance" disabled="disabled">Requirements Compliance</button>
            </div>
            <div class="row">
                <button class="btn btn-default btn-default-orange nwcuzCustom-002" id="btnHealthDec" disabled="disabled">Health Declaration</button>
            </div>
         
        </div>
    </div>


