                            <div class="lbl-title-note-land">* Green ribbon on Blocks/Floors indicates unit availability.</div>
                            <iframe id="unitMap" style=""></iframe>
                            <div class="lvl-b-box" style="display:none;">
                                <div class="lvl-c-box" id="boxcont" style="">
                                </div>
                            </div>
                            <div class="row-c"></div>
                            <div class="row-c" id="divBulkDtls" style="">
                                <div class="BulkDtls-c">
                                    <div class="leg-note left">
                                        <div id="btnViewBulkDtls" class="btn btn-type1">View Bulk Unit Details</div>
                                    </div>
                                    <div class="leg-note right">
                                        <div class="leg-note-ul bulkul">
                                            <div class="leg-note-li">
                                                <span class="leg-note-lbl bulkcnt">Total No. of Units Selected: </span>
                                                <span id="cntUnits" class="leg-note-ctr bulkcnt">0</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row-c" style="">
                                <div id="btnGenAvailability" class="btn btn-type1">Generate Availability Summary</div>
                            </div>
                            <div class="row-c" id="secAvailSumUA">
                                <div class="lbl-title-list">Inventory Availability Summary</div>
                                <div class="btn btn-type1" id="btnExport">Export to Excel</div>
                                <br />
                                <table id="tblAvlSum" class="tbl-tmp1 --has-width">
                                    <tr>
                                        <th>Project</th>
                                        <th>Block/Floor</th>
                                        <th>Lot/Unit/Slot</th>
                                        <th>Type</th>
                                        <th>Class</th>
                                        <th>Total Area</th>
                                        <th>Selling Price</th>
                                        <th>Reservation Amount</th>
                                    </tr>
                                    <tr>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td class="--currency"></td>
                                        <td class=" --currency"></td>

                                    </tr>
                                </table>
                            </div>
                        </div>
                    </fieldset>
                </div>

 
            <div class="side-popup clicked-lvl">
                <div class="clicked-lvl-preview">
                    <div class="nk-carousel nk-image-viewer">
                        <div class="nk-carousel-slider" id="nkSlider2" style="">
                            <!-- Slides -->
                            <!-- Navigation Arrows -->
                            <!-- Navigation Dots -->
                            <div class="btn-carousel-dotnav-cont"></div>
                            <!-- <div class="nk-carousel-slide-title autohide">Asiera Homes Pampanga</div> -->
                        </div>
                        <div class="btn btn-carousel-actnav btn-carousel-actnav-prev show"></div>
                        <div class="btn btn-carousel-actnav btn-carousel-actnav-next show"></div>
                        <div class="nk-carousel-thumb-cont" style="width: 600px;">
                            <div class="nk-carousel-thumb-cont-wrap"></div>
                        </div>
                    </div>
                </div>
                <div id="btnHold" class="btn btn-type1 btn-hold">Hold</div>
                <div id="btnReserveNow" class="btn btn-type1 btn-reserve">Reserve Now</div>
                <div class="row-c-m" id="preview">
                    <div class="preview-info-c" id="secMultipleSP" style="display:none;">
                        <div class="SpFirst custPage"><<</div>
                        <div class="SpPrev custPage"><</div>
                        <div class="SpCnt"><input id='curCnt' value="1" /> of ..</div>
                        <div class="SpLast custPage">>></div>
                        <div class="SpNext custPage">></div>
                    </div>
                    <div class="preview-info-c">
                        <div class="preview-info-list-hdr">Unit Details</div>
                        <ul>
                            <li>
                                <div>Unit/Lot</div>
                                <div id="unitno">...</div>
                                <div id="ucode" style="display:none"></div>
                            </li>
                            <li>
                                <div>Type</div>
                                <div id="type">...</div>
                                <div id="typecode" style="display:none"></div>
                            </li>
                            <li class="grptotarea">
                                <div>Total Area</div>
                                <div id="totarea">...</div>
                            </li>
                            <li class="grplotarea">
                                <div>Lot Area</div>
                                <div id="lotarea">...</div>
                            </li>
                            <li class="grpunitarea">
                                <div>Unit Area</div>
                                <div id="unitarea">...</div>
                            </li>
                            <li>
                                <div>Class</div>
                                <div id="clss">...</div>
                                <div id="clsscode" style="display:none"></div>
                            </li>
                            <li>
                                <div>Reservation Amount</div>
                                <div id="rsrvamt">...</div>
                            </li>
                        </ul>
                    </div>
                    <div class="preview-info-c">
                        <div class="preview-info-list-hdr">Selling Price</div>
                        <ul>
                            <li>
                                <div>PHP</div>
                                <div id="amtp">0.00</div>
                                <div id="amtphp" style="display:none"></div>
                            </li>
                            <li>
                                <div>USD</div>
                                <div id="amtu">0.00</div>
                            </li>
                            <li>
                                <div>EUR</div>
                                <div id="amte">0.00</div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Booking Summary -->
    <div id="secBookingSummary" class="content-c" style="display: none;">
        <div class="row-c row-booksum">
            <div class="summary-header">
                <div class="summary-header-sec left">
                    <div class="summary-hdr-title">MY BOOKING SUMMARY</div>
                </div>
                <div class="summary-header-sec right">
                    <div class="hdr-tile">
                        <div class="hdr-tile-title">On-Hold</div>
                        <div class="hdr-tile-count" id="cntHoldbs">..</div>
                    </div>
                    <div class="hdr-tile">
                        <div class="hdr-tile-title">Reserved</div>
                        <div class="hdr-tile-count" id="cntReservedbs">..</div>
                    </div>
                    <div class="hdr-tile">
                        <div class="hdr-tile-title">Re-Opened</div>
                        <div class="hdr-tile-count" id="cntReopenedbs">..</div>
                    </div>
                    <div class="hdr-tile">
                        <div class="hdr-tile-title">Sold/Booked</div>
                        <div class="hdr-tile-count" id="cntSoldbs">..</div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row-c row-custom-note">
            <div class="col">
                <label class="lbl-hdr-text" style="">Project</label>
                <select id="cmbProject" class="lbl-hdr-sel" style="width:220px">
                    <option value="all">All projects</option>
                </select>
            </div>
            <br />
            <b><i>Note:	Click on LOT/UNIT/SLOT column to view the booking details.</i></b>
            <br />
            <br />
        </div>
        <div class="row-c row-tbl-c row-tbl-onhold row-lastborder-c" id="onhold">
            <div class="lbl-title-list" id="cntHoldtbl">On-Hold - ..</div>
            <div class="lbl-title-list lbl-title-list-sub" id="cntHoldtcp">TCP - ..</div>
            <table id="tblBookSumOnHold" class="tbl-tmp1"></table>
        </div>
        <div class="row-c row-tbl-c row-tbl-reserved" id="reserved">
            <div class="lbl-title-list" id="cntReservedtbl">Reserved - ..</div>
            <div class="lbl-title-list lbl-title-list-sub" id="cntReservedtcp">TCP - ..</div>
            <table id="tblBookSumReserved" class="tbl-tmp1"></table>
        </div>
        <div class="row-c row-tbl-c row-tbl-reopened" id="reopened">
            <div class="lbl-title-list" id="cntReopenedtbl">Re-Opened - ..</div>
            <div class="lbl-title-list lbl-title-list-sub" id="cntReopenedtcp">TCP - ..</div>
            <table id="tblBookSumReopened" class="tbl-tmp1"></table>
        </div>
        <div class="row-c row-tbl-c row-tbl-booked" id="sold">
            <div class="lbl-title-list" id="cntSoldtbl">Sold/Booked - ..</div>
            <div class="lbl-title-list lbl-title-list-sub" id="cntSoldtcp">TCP - ..</div>
            <table id="tblBookSumSold" class="tbl-tmp1"></table>
        </div>
    </div>
    <!-- Commission Summary -->
    <div id="secCommissionSummary" class="content-c" style="display: none;">
        <div class="row-c">
            <div class="lbl-title-list">My Commissions</div>
        </div>
        <div class="row-c">
            <input name="grp1" class="rdo" style="width: 50px" id="cbAsOfDate" onclick="ChangeCommisionDate('1')" type="radio" />
            <label id="lblAsOfDate" class="lblrdo lbl-hdr-text" style="min-width: 50px;" for="cbAsOfDate">As of Date </label>
            <div class="commdates">
                <input id="txtAsOfDate" class="txtFull txtbox txtbox-date txtAsOfDate" type="date" style="min-width: 180px; max-width: 180px;" />
            </div>
            <div class="commright">
                <label class="lbl-hdr-text" style="">Project</label>
                <select id="cmbProjectCOMM" class="lbl-hdr-sel" style="width:220px">
                    <option value="all">All projects</option>
                </select>
            </div>
        </div>
        <div class="row-c">
            <input name="grp1" class="rdo" style="width: 50px" id="cbCutoffDate" onclick="ChangeCommisionDate('2')" type="radio" />
            <label id="lblCutoffDate" class="lblrdo lbl-hdr-text" style="min-width: 50px;" for="cbCutoffDate">Cut-off Date </label>
            <div class="commright">
                <label class="lbl-hdr-text" style="">Customer</label>
                <select id="cmbCustomerCOMM" class="lbl-hdr-sel" style="width:220px">
                    <option value="all">All customers</option>
                </select>
            </div>
        </div>
        <div class="row-c">
            <div class="commdates">
                <label class="lbl-hdr-text" id="commfrom" style="">From</label>
                <input id="txtCOFrom" class="txtFull txtbox txtbox-date txtCOFrom" type="date" style="min-width: 180px; max-width: 180px;" />
            </div>
        </div>
        <div class="row-c">
            <div class="commdates">
                <label class="lbl-hdr-text" id="commto" style="">To</label>
                <input id="txtCOTo" class="txtFull txtbox txtbox-date txtCOTo" type="date" style="min-width: 180px; max-width: 180px;" />
            </div>
        </div>
        <div class="row-c">
            <div class="lbl-title-list">Commission Ledger - Summary</div>
            <div class="list-items-c">
                <div class="summary-c">
                    <div class="row">
                        <div class="col">Total no. of Accounts</div>
                        <div class="col" id="commTotalNo">0</div>
                    </div>
                    <div class="row">
                        <div class="col">Total Commission Amount</div>
                        <div class="col" id="commTotalAmt">0.00</div>
                    </div>
                    <div class="row">
                        <div class="col">Total Commission Received</div>
                        <div class="col" id="commTotalRcvd">0.00</div>
                    </div>
                    <div class="row">
                        <div class="col">Total Commission for Pick-up</div>
                        <div class="col" id="commTotalForPickup">0.00</div>
                    </div>
                    <div class="row">
                        <div class="col">Balance</div>
                        <div class="col" id="commBalance">0.00</div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row-c">
            <div class="lbl-title-list">Commission Ledger - Detailed</div>
            <table id="tblComSum" class="tbl-tmp1 --has-width"></table>
        </div>
    </div>
    <!-- Transaction Summary -->
    <div id="secTransactionSummary" class="content-c" style="display: none;">
        <div class="row-c">
            <div class="btn btn-cancel">Back</div>
            <div class="lbl-title-list" id="mi-title">My Transactions Summary</div>
        </div>
        <div class="row-c">
            <iframe id="iframeTransactions" src="" class="f-loader"></iframe>
        </div>
    </div>

    <!-- Menu Item: Holding-->
    <div id="secHoldingFrame" class="content-c f-loader-c" style="display: none;">
        <iframe id="iframeHolding" src="" class="f-loader"></iframe>
    </div>

    <!-- Floaters -->
    <div id="pdlgProject" class="pdlg" style="display: none;">
        <div class="pdlg-box">
            <div class="pdlg-hdr">
                <div class="pdlg-title">Select another Project/Phase/Tower</div>
            </div>
            <div class="pdlg-content">
                <div class="pdlg-ul" id="pdlg-container">
                </div>
                <div class="pdlg-ul" id="pdlg-msg" style="display:none">
                    <div class="messagecont" style="white-space:pre-wrap;">...</div>
                </div>
            </div>
            <div class="pdlg-actions">
                <div class="btn btn-cancel">Back</div>
            </div>
        </div>
    </div>

    <!-- iFrames -->
    <div id="popup" class="iframe_main" canbefullwidth="true" fw-state="normal" style="display: none;">
        <div class="iframe_box">
            <div class="iframe-actions">
                <div class="btn btn-cancel">Back</div>
            </div>
