@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "Activity Details Entry";
    Layout = NoahWebLib.nwSystem.GetDefaultLayout();
}

<head>
    <script src="~/WFM/DocumentEntry/WFMActivityRqrmntComplianceEntry/WFMActivityRqrmntComplianceEntry.js"></script>
    <link href="~/WFM/DocumentEntry/WFMActivityRqrmntComplianceEntry/WFMActivityRqrmntComplianceEntry.css" rel="stylesheet" />
</head>

@*ToolBox*@
<div id="noah-webui-Toolbox" class="nk-toolbar-c">
    <div id="noah-webui-default-New" class="btn-tb-action btn-tb-new noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">New</div>
    </div>
    <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-save noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Save</div>
    </div>
    <div id="noah-webui-default-Delete" class="btn-tb-action btn-tb-delete noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Delete</div>
    </div>
    <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Refresh</div>
    </div>
    <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Inquire</div>
    </div>
    <div id="noah-webui-default-Process" class="btn-tb-action btn-tb-process noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Process</div>
    </div>
    @*<div id="noah-webui-default-Import" class="btn-tb-action btn-tb-import noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Import</div>
    </div>*@
    @*<div id="noah-webui-default-Export" class="btn-tb-action btn-tb-export noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Export</div>
    </div>
    <div id="noah-webui-default-Print" class="btn-tb-action btn-tb-print noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Print</div>
    </div>*@
</div>

@*Main*@
<input type="hidden" id="txtBase64Img" />
<input type="hidden" id="txtBase64ImgType" />
<input type="hidden" id="txtBase64ImgPath" />

<div class="container">
    <div class="container-flex">
        <div class="row row-parent">
            <div class="col col-6 col-parent">


                <div class="row with-label-lookup">
                    <div class="col col-5">
                        <label class="lbltxt">Transaction/Activity Type<span class="nwRequiredField">*</span></label>
                    </div>
                    <div id="lugTranType" class="col col-7 col-parent adisabled lookups">
                        <div class="col col-4 conval">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8 conval">
                            <input type="text" name="" class="txtbox txtdesc descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup LookUp"></div>
                        </div>
                    </div>
                </div>

                <div class="row with-label-lookup">
                    <div class="col col-5">
                        <label class="lbltxt">Location with Accountable Forms<span class="nwRequiredField">*</span></label>
                    </div>
                    <div id="lugLocForm" class="col col-7 col-parent adisabled lookups">
                        <div class="col col-4 conval">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8 conval">
                            <input type="text" name="" class="txtbox txtdesc descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup LookUp"></div>
                        </div>
                    </div>
                </div>






                @*start of dynamic field*@
                <div id="field1" class="row">

                </div>
                <div id="field2" class="row">
                    
                </div>
                <div id="field3" class="row">
                    
                </div>
                <div id="field4" class="row">
                    
                </div>
                <div id="field5" class="row">
                    
                </div>
                <div id="field6" class="row">
                    
                </div>
                <div id="field7" class="row">
                    
                </div>
                <div id="field8" class="row">
                    
                </div>
                <div id="field9" class="row">
                    
                </div>
                <div id="field10" class="row">
                    
                </div>
                <div id="field11" class="row">
                    
                </div>
                <div id="field12" class="row">
                    
                </div>
                <div id="field13" class="row">
                    
                </div>
                <div id="field14" class="row">
                    
                </div>
                <div id="field15" class="row">
                    
                </div>
                <div id="field16" class="row">
                    
                </div>
                <div id="field17" class="row">
                    
                </div>
                <div id="field18" class="row">
                    
                </div>
                <div id="field19" class="row">
                    
                </div>
                <div id="field20" class="row">
                    
                </div>
                @*end of dynamic field*@

            </div>
            <div class="col col-6 col-parent">

                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Transaction No.</label>
                    </div>
                    <div class="col col-7">
                        <input id="txtDocno" type="text" name="" class="txtbox" disabled>
                        <input id="txtRefDocno" class="txtbox nwHide" type="text" disabled />
                        <input id="txtCC" class="txtbox nwHide" type="text" disabled />
                        <input id="txtPC" class="txtbox nwHide" type="text" disabled />
                        <input id="txtAmount" class="txtbox nwHide" type="text" disabled />
                        <input id="txtmaxTag" class="txtbox nwHide" type="text" disabled />
                        <input id="txtVendor" class="txtbox nwHide" type="text" disabled />
                        <input id="txtRefno" class="txtbox nwHide" type="text" disabled />
                        <input id="txtBasePercentage" class="txtbox nwHide" type="text" disabled />
                    </div>
                </div>

                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Transaction Date</label>
                    </div>
                    <div class="col col-7">
                        <input id="txtDocDate" type="text" name="" class="txtbox" disabled>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Date Submitted</label>
                    </div>
                    <div class="col col-7">
                        <input id="txtDateSubmitted" type="text" name="" class="txtbox" disabled>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Date Posted</label>
                    </div>
                    <div class="col col-7">
                        <input id="txtDatePosted" type="text" name="" class="txtbox" disabled>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Remarks</label>
                    </div>
                    <div class="col col-7">
                        <textarea id="txtRemarks" class="txtarea autoresize" rows="4" cols="42" maxlength="500" disabled></textarea>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Document Status</label>
                    </div>
                    <div class="col col-7">
                        <input id="txtDocStatus" type="text" name="" class="txtbox" disabled>
                        <input id="txtDocStatusCode" type="hidden" name="" class="txtbox" disabled>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Reason for Disapproval</label>
                    </div>
                    <div class="col col-7">
                        <div class="col col-5">
                            <input id="idvallugRsnForDisAproval" type="text" name="" class="txtbox idval" disabled>
                        </div>
                        <div class="col col-7">
                            <input id="descvallugRsnForDisAproval" type="text" name="" class="txtbox txtdesc" value="" disabled>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-5">
                        <label class="lbltxt">Disapproval Remarks</label>
                    </div>
                    <div class="col col-7">
                        <textarea id="txtDisapprvRemarks" maxlength="500" class="txtarea autoresize" rows="5" cols="50" disabled></textarea>
                    </div>
                </div>
                <div class="row">
                    <div class="col col-5">
                    </div>
                    <div class="col col-7">
                        <div class="btn btn-default fullwidth" id="btnReviewAttach">Review Attachment(s)</div>
                    </div>
                </div>
                <div class="row">
                    <div class="col col-5">
                    </div>
                    <div class="col col-7">
                        <div class="btn btn-default fullwidth" id="btnReqComp">Requirements Compliance</div>
                    </div>
                </div>


            </div>
        </div>
    </div>

</div>

