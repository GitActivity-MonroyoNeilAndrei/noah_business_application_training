@inherits System.Web.Mvc.WebViewPage
@{

    ViewBag.Title = "PopupWindows";
}

<div id="nwInvoiceType" nwTitle="Invoice Type" class="nkPopupCon">
    <div class="container-flex padding-10">
        <div class="row row-parent">
            <div class="col col-12 col-parent child-centered">
                <div class="row">
                    <div class="col col-12">
                        <label class="lbltxt">Please select Invoice Type</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col col-12">
                        <select id="cbInvoiceType" class="cmb fullwidth">
                            <option value="INVREG">Regular Charges</option>
                        </select>
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                    </div>
                </div>
                <div class="row">
                    <div class="col col-4">
                        <div id="btnProceed" class="btn btn-default btnMedium fullwidth">
                            Proceed >>
                        </div>
                    </div>
                    <div class="col col-8">
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>


