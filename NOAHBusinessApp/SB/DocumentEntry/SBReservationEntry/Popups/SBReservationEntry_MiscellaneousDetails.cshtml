@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "Reservation Entry";
}



<div id="nwMiscellaneousDtls" class="modal-s">
    <div class="modal-box-bg">
        <div class="modal-box-s"></div>
        <div class="modal-hdr">
            <div class="btn btn-modal-back" nk-fx-click="press" title="Back"><span></span></div>
            <div class="modal-hdr-title">Miscellaneous Feess Details</div>
            <div class="btn btn-modal-rescale-s" nk-fx-click="press" title="Rescale"></div>
        </div>

        <div class="nk-modal-body">


            <div id="noah-webui-Toolbox_Refbase" class="nk-toolbar-c">
                <div id="btnSaveMiscellaneous" class="btn-tb-action btn-tb-save">
                    <div class="_icon"></div>
                    <div class="_lbl">Save</div>
                </div>
            
            </div>

            <div class="row">
                <div class="col col-0">
                    <label class="lbltxt">Rate</label>
                </div>
                <div class="col col-1">
                    <input type="text" id="txtRate" class="isNumber numC noah-webui-disabled" disabled="" autocomplete="off">
                </div>
                <div class="col col-2">
                </div>
                <div class="col col-1">
                    <label class="">Miscellaneous Amount</label>
                </div>
                <div class="col col-1 ">
                    <input type="text" id="txtMiscAmt" class="isNumber numC noah-webui-disabled" disabled>
                </div>
                <div class="col col-2">
                 
                </div>
                <div class="col col-4">
                </div>
            </div>





            <div class="row row-parent padding-10">
                <div class="col col-12">
                    <div id="nwGridMiscellaneousFee" class="table-wrapper">
                    </div>
                </div>
            </div>
    



        </div>

    </div>
</div>







