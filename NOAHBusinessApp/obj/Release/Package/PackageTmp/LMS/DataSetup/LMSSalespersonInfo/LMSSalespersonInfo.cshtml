                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>

                @*Other ID no.*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Other ID no.</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtOtherIDNo" class="nwCuz005" type="text" maxlength="80" />
                    </div>
                </div>

            </div>
            <div class="col col-0-25 col-parent"></div>
            @*MIDDLE*@
            <div class="col col-3-75 col-parent">

                <label class="lbltxt"><b><i><u>Salesperson Address</u></i></b></label>

                @*Location Code*@
                @*<div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Location Code</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtLocCode" class="" type="text" />
                    </div>
                </div>*@

                @*Location Type*@
                @*<div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Location Type</label>
                    </div>
                    <div class="col col-8 col-parent lookups" id="lugLocType">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>*@

                @*Full Address*@
                <div class="row">
                    <div class="col col-4">
                        <div class="chk-all-con">
                            <input type="checkbox" id="chkFullAddress" class="chk-all-s unchecked" check-all-for="disapprove">
                            <label class="lbl-chkall" for="chkFullAddress">Full Address<span class="nwRequiredField" id="reqFA"> *</span></label>
                        </div>
                    </div>
                    <div class="col col-8">
                        <textarea id="txtFullAddress" maxlength="255" row="3"></textarea>
                    </div>
                </div>

                @*Unit No.*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Unit No.</label>
                    </div>
                    <div class="col col-1">
                        <input id="txtUnitNo" class="" type="text" maxlength="12" />
