                    <div class="col col-4">
                        <label class="lbltxt">VAT Reg TIN<span class="nwRequiredField">*</span></label>
                    </div>
                    <div class="col col-8">
                        <input id="txtVat" class="nwNoSpace" type="text" minlength="9" maxlength="12" />
                    </div>
                </div>

                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Non-VAT Reg TIN<span class="nwRequiredField">*</span></label>
                    </div>
                    <div class="col col-8">
                        <input id="txtNonVat" class="nwNoSpace" type="text" minlength="9" maxlength="12" />
                    </div>
                </div>

