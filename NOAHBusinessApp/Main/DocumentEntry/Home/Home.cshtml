                    <div class="nk-carousel-slider" id="nkSlider1" style="">
                        <!-- Slides -->
                        <!-- Navigation Arrows -->
                        <!-- Navigation Dots -->
                        <div class="btn-carousel-dotnav-cont"></div>
                        <!-- <div class="nk-carousel-slide-title autohide">Asiera Homes Pampanga</div> -->
                    </div>
                    <div class="btn btn-carousel-actnav btn-carousel-actnav-prev show"></div>
                    <div class="btn btn-carousel-actnav btn-carousel-actnav-next show"></div>
                    <div class="nk-carousel-thumb-cont" style="width: 600px;"><div class="nk-carousel-thumb-cont-wrap"></div></div>
                </div>
            </div>

   
        </div>

        <!-- Availability Chart -->

        <div id="secAvailabilityChart" class="content-c with-back-btn" style="display: none; min-height: 500px;">
 
            <div class="row-c">
                <div class="lbl-title-list-center">Unit Inventory Availability</div>
                <div class="lbl-title-note"></div>
            </div>
            <div class="row-c">
                <div class="leg-note-c">
                    <div class="leg-note left">
                        <div class="leg-note-hdr">Status Legend</div>
                        <div class="leg-note-ul">
