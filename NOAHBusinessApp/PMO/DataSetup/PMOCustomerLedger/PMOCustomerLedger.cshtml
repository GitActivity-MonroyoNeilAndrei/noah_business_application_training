                    <div class="row">
                        <div class="col col-12 col-parent child-centered">
                            <div class="row">
                                <div id="nwGridPopupDetails" class="col col-12">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

