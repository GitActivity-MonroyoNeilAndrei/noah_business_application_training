            <div id="noah-webui-default-first" class="noah-webui-Toolbox-Item-Icon BN-button">
                <div></div>
            </div>
            <div id="noah-webui-default-previous" class="noah-webui-Toolbox-Item-Icon BN-button">
                <div></div>
            </div>
          
            <div id="noah-webui-default-next" class="noah-webui-Toolbox-Item-Icon BN-button">
                <div></div>
            </div>
            <div id="noah-webui-default-last" class="noah-webui-Toolbox-Item-Icon BN-button">
                <div></div>
            </div>
        </div>

        <div class="nk-toolbar-btn noah-webui-Toolbox">
            <div id="noah-webui-default-New" class="btn-tb-action btn-tb-new noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">New</div>
            </div>
            <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-save noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Save</div>
            </div>
            <div id="noah-webui-default-Delete" class="btn-tb-action btn-tb-delete noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Delete</div>
            </div>
            <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Refresh</div>
            </div>
            <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Inquire</div>
            </div>
            <div id="noah-webui-default-Process" class="btn-tb-action btn-tb-process noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Process</div>
            </div>

            <div id="btnProcessCancellation" class="btn-tb-action btn-tb-process noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Consolidate PR</div>
            </div>

        </div>
    </div>




    <div class="nk-container-flex">

        <div class="row row-parent">

            <div class="col col-6">

                @*Location with Accountable Forms*@
                <div class="row">
                    <div class="row with-label-lookup">
                        <div class="col col-4">
                            <label class="lbltxt">Location with Accountable Forms<span class="nwRequiredField">*</span></label>
                        </div>
                        <div class="col col-1">
                        </div>

                        <div id="lugLocation" class="col col-6 col-parent lookups">
                            <div class="col col-4 conval">
                                <input type="text" name="" class="txtbox idval">
                            </div>
                            <div class="col col-8 conval">
                                <input type="text" name="" class="txtbox txtdesc descval">
                                <div class="btn btn-sm-default btn-sm-default-lookup LookUp"></div>
                            </div>
                        </div>

                        <div class="col col-1">
                        </div>
                    </div>
                </div>

                @*PR Consolidation No.*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">PR Consolidation No.<span class="nwRequiredField">*</span></label>
                    </div>
                    <div class="col col-1">
                    </div>
                    <div class="col col-6">
                        <div id="settingstabs" class="fullwidth">
                        </div>
                    </div>
                    <div class="col col-1">
                    </div>
                </div>

                @*Load Details*@
                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-1">
                    </div>
                    <div class="col col-6">
                        <div class="btn btn-default btn-default-orange fullwidth" id="btnLoadDtls">
                            Load Details
                        </div>
                    </div>
                    <div class="col col-1">
                    </div>
                </div>

                @*Upload Canvass Details*@
                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-1">
                    </div>
                    <div class="col col-6">
                        <div class="btn btn-default btn-default-orange fullwidth" id="btnUploadCanvass">
                            Upload Canvass Details
                        </div>
                    </div>
                    <div class="col col-1">
                    </div>
                </div>

                @*Validation List*@
                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-1">
                    </div>
                    <div class="col col-6">
                        <div class="btn btn-default btn-default-orange fullwidth" id="btnValList">
                            Validation List
                        </div>
                    </div>
                    <div class="col col-1">
                    </div>
                </div>

                @*Requirements Compliance*@
                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-1">
                    </div>
                    <div class="col col-6">
                        <div class="btn btn-default btn-default-orange fullwidth" id="btnReqComp">
                            Requirements Compliance
                        </div>
                    </div>
                    <div class="col col-1">
                    </div>
                </div>

            </div>

            <div class="col col-6">

                @*Document No.*@
