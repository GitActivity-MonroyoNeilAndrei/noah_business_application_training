@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "Company";
    Layout = NoahWebLib.nwSystem.GetDefaultLayout();
}

<head>
    <script src="~/SA/DataSetup/SACompany/SACompany.js"></script>
    <link href="~/SA/DataSetup/SACompany/SACompany.css" rel="stylesheet" />
</head>

<div id="noah-webui-Toolbox" class="nk-toolbar-c">
    <div id="noah-webui-default-New" class="btn-tb-action btn-tb-new">
        <div class="_icon"></div>
        <div class="_lbl">New</div>
    </div>
    <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-save">
        <div class="_icon"></div>
        <div class="_lbl">Save</div>
    </div>

    <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-delete">
        <div class="_icon"></div>
        <div class="_lbl">Delete</div>
    </div>
    <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh">
        <div class="_icon"></div>
        <div class="_lbl">Refresh</div>
    </div>
    <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire">
        <div class="_icon"></div>
        <div class="_lbl">Inquire</div>
    </div>
    <div id="noah-webui-default-Export" class="btn-tb-action btn-tb-export">
        <div class="_icon"></div>
        <div class="_lbl">Export</div>
    </div>
</div>



<div class="container-flex">
    <div class="row row-parent padding-10">
        <div class="col col-5 col-parent">
        
            <div class="row">
                <div class="col col-3">
                    <label class="lbltxt">Code</label>
                </div>
                <div class="col col-9">
                    <input type="text" name="" id="inCode" class="txtbox nwCode nwUpper" maxlength="12" >
                </div>
            </div>

            <div class="row">
                <div class="col col-3">
                    <label class="lbltxt">Description</label>
                </div>
                <div class="col col-9">
                    <input type="text" name="" id="inDesc">
                </div>
            </div>


            <div class="row with-label-lookup">
                <div class="col col-3">
                    <label class="lbltxt">Database</label>
                </div>
                <div class="col col-9 col-parent">
                    <div class="col col-5">
                        <input type="text" name="" class="txtbox txtcode" id="idvallugCode">
                    </div>
                    <div class="col col-7">
                        <input type="text" name="" class="txtbox txtdesc txtlookup" id="descvallugCode">
                        <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                    </div>
                </div>
            </div>


        </div>
        <div class="col col-7 col-parent"></div>
    </div>

   



    <div class="row row-parent child-centered">
        <div class="col col-12">
            <div class="tabs tabs-lbl-bold" nk-tabs-style="3">
                <div class="tabs-content">
                    <input id="nkTabs_e_1" type="radio" class="tabs-rdb" checked="" name="tabs-e">
                    <label for="nkTabs_e_1" class="tabs-lbl">Module Setup</label>
                    <div class="tabs-text">
                        <div class="row row-parent padding-x-10">
                            <div class="col col-12 col-parent">
                                <div id="tblWithTabsWrapperSample3" class="table-tabs-wrapper rescale-ready" nk-table-style="2">
                                 


                                    <div class="col col-8 col-parent">
                                        <div class="row">
                                            <div class="col col-12 col-parent">
                                                <div class="col col-3">
                                                    <div class="btn btn-default fullwidth">Add Desktop Module</div>
                                                </div>
                                                <div class="col col-3">
                                                    <div class="btn btn-default fullwidth">Copy Desktop Module</div>
                                                </div>
                                           <div class="col col-6"></div>
                                            </div>
                                        </div>
                                    </div>



                                    <div id="panelTblTabColSticky" class="table-wrapper">
                                        <table class="table" id="grid-sticky" sid="nkTable0">
                                            <thead>
                                                <tr>
                                                    <th data-type="str" scope="col" class="tbl-row-num"></th>
                                                    <th data-type="str" scope="col"></th>
                                                    <th data-type="str" scope="col">Desktop Module Code</th>
                                                    <th data-type="str" scope="col">Desktop Module Description</th>
                                                    <th data-type="str" scope="col">Web Module Code</th>
                                                    <th data-type="str" scope="col">Web Module Description</th>
                                              
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>

                                                    <th scope="row" class="tbl-row-num" row-th="">1</th>
                                                    <td row-th="">

                                                        <div class="btn-icon-action btn-icon-delete" title="Delete">
                                                            <svg xmlns="http://www.w3.org/2000/svg" width="22px" height="22px" class="nw-svg-d" viewBox="0 0 22 22">
                                                                <path class="nw-svg-d-a" d="M16,7h1V17a3,3,0,0,1-3,3H8a3,3,0,0,1-3-3V7H6V17a2,2,0,0,0,2,2h6a2,2,0,0,0,2-2ZM10,9H9v7h1Zm3,0H12v7h1Zm1-5V2H8V4H3V5H19V4Z"></path>
                                                            </svg>
                                                        </div>


                                                    </td>
                                                    <td row-th="Desktop Module Code"><span class="wrap-td"></span></td>
                                                    <td scope="col" row-th="Desktop Module Description"><span class="wrap-td"></span></td>
                                                    <td scope="col" row-th="Web Module Code"><span class="wrap-td"></span></td>
                                                    <td scope="col" row-th="Web Module Description"><span class="wrap-td"></span></td>
                                                    
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>


                                

                                </div>
                            </div>
                        </div>



                        <div class="col col-8 col-parent">
                            <div class="row">
                                <div class="col col-12 col-parent">
                                    <div class="col col-3">
                                        <div class="btn btn-default fullwidth">Add Web Module</div>
                                    </div>
                                    <div class="col col-3">
                                        <div class="btn btn-default fullwidth">Copy Web Module</div>
                                    </div>
                                    <div class="col col-6"></div>
                                </div>
                            </div>
                        </div>
                        <div id="panelTblTabColSticky2" class="table-wrapper">
                            <table class="table" id="grid-sticky" sid="nkTable0">
                                <thead>
                                    <tr>
                                        <th data-type="str" scope="col" class="tbl-row-num"></th>
                                        <th data-type="str" scope="col"></th>
                                        <th data-type="str" scope="col">Web Module Code</th>
                                        <th data-type="str" scope="col">Web Module Description</th>


                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>

                                        <th scope="row" class="tbl-row-num" row-th="">1</th>
                                        <td row-th="">

                                            <div class="btn-icon-action btn-icon-delete" title="Delete">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="22px" height="22px" class="nw-svg-d" viewBox="0 0 22 22">
                                                    <path class="nw-svg-d-a" d="M16,7h1V17a3,3,0,0,1-3,3H8a3,3,0,0,1-3-3V7H6V17a2,2,0,0,0,2,2h6a2,2,0,0,0,2-2ZM10,9H9v7h1Zm3,0H12v7h1Zm1-5V2H8V4H3V5H19V4Z"></path>
                                                </svg>
                                            </div>


                                        </td>


                                        <td scope="col" row-th="Web Module Code"><span class="wrap-td"></span></td>
                                        <td scope="col" row-th="Web Module Description"><span class="wrap-td"></span></td>

                                    </tr>
                                </tbody>
                            </table>
                        </div>

                    </div>
                    <!--  -->
                    <input id="nkTabs_e_2" type="radio" class="tabs-rdb" name="tabs-e">
                    <label for="nkTabs_e_2" class="tabs-lbl">User Setup</label>
                    <div class="tabs-text">
                        <div class="row row-parent padding-x-10">
                            <div class="col col-12 col-parent">
                                <div id="tblWithTabsWrapperSample3" class="table-tabs-wrapper rescale-ready" nk-table-style="2">
                                    <div id="panelTblTabColSticky" class="table-wrapper">
                                        <table class="table" id="grid-sticky" sid="nkTable1">
                                            <thead>
                                                <tr>
                                                    <th data-type="str" scope="col" class="tbl-row-num"></th>
                                                    <th data-type="str" scope="col">User Code</th>
                                                    <th data-type="str" scope="col">User Description</th>
                                                 
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <th scope="row" class="tbl-row-num" row-th="">1</th>
                                                    <td row-th="">

                                                        <div class="btn-icon-action btn-icon-delete" title="Delete">
                                                            <svg xmlns="http://www.w3.org/2000/svg" width="22px" height="22px" class="nw-svg-d" viewBox="0 0 22 22">
                                                                <path class="nw-svg-d-a" d="M16,7h1V17a3,3,0,0,1-3,3H8a3,3,0,0,1-3-3V7H6V17a2,2,0,0,0,2,2h6a2,2,0,0,0,2-2ZM10,9H9v7h1Zm3,0H12v7h1Zm1-5V2H8V4H3V5H19V4Z"></path>
                                                            </svg>
                                                        </div>


                                                    </td>
                                                  
                                                    <td row-th=""> </td>
