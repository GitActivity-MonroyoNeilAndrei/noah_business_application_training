@inherits System.Web.Mvc.WebViewPage
@{
     ViewBag.Title = "Module Web";
     Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    <script src="~/SA/DataSetup/SAModuleWeb/SAModuleWeb.js"></script>
    <link href="~/SA/DataSetup/SAModuleWeb/SAModuleWeb.css" rel="stylesheet" />
</head>

     <div id="noah-webui-Toolbox" class="nk-toolbar-c">
         <div id="noah-webui-default-New" class="btn-tb-action btn-tb-new">
             <div class="_icon"></div>
             <div class="_lbl">New</div>
         </div>
         <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-save">
             <div class="_icon"></div>
             <div class="_lbl">Save</div>
         </div>
     
            <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-delete">
                <div class="_icon"></div>
                <div class="_lbl">Delete</div>
            </div>
            <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh">
                <div class="_icon"></div>
                <div class="_lbl">Refresh</div>
            </div>
            <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire">
                <div class="_icon"></div>
                <div class="_lbl">Inquire</div>
            </div>
            <div id="noah-webui-default-Process" class="btn-tb-action btn-tb-process">
                <div class="_icon"></div>
                <div class="_lbl">Process</div>
            </div>
            <div id="noah-webui-default-Export" class="btn-tb-action btn-tb-export">
                <div class="_icon"></div>
                <div class="_lbl">Export</div>
            </div>
            <div id="noah-webui-default-Import" class="btn-tb-action btn-tb-import">
                <div class="_icon"></div>
                <div class="_lbl">Import</div>
            </div>
            <div id="noah-webui-default-Print" class="btn-tb-action btn-tb-print">
                <div class="_icon"></div>
                <div class="_lbl">Print</div>
            </div>
            <div class="btn-tb-action btn-tb-update">
                <div class="_icon"></div>
                <div class="_lbl">Update</div>
            </div>
        </div>



<div class="container-flex">
    <div class="row row-parent padding-10">
        <div class="col col-4 col-parent">
            <div class="row">
                <div class="col col-3">
                    <label class="lbltxt">Code</label>
                </div>
                <div class="col col-9">
                    <input type="text" name="" id="inCode">
                </div>
            </div>

            <div class="row">
                <div class="col col-3">
                    <label class="lbltxt">Description</label>
                </div>
                <div class="col col-9">
                    <input type="text" name="" id="inDesc">
                </div>
            </div>

            <div class="row">
                <div class="col col-3">
                    <label class="lbltxt">App Name</label>
                </div>
                <div class="col col-9">
                    <input type="text" name="" id="inAppName">
                </div>
            </div>

          
            <div class="row">
                <div class="col col-3">
                    <label class="lbltxt">Application Version</label>
                </div>
                <div class="col col-9 col-parent">
                    <div class="col col-12">
                        <select class="cmb fullwidth" id="sel_AppVer">
                            <option>Custom</option>
                        
                        </select>
                    </div>
                   
                </div>
            </div>

        

        </div>
        <div class="col col-8"></div>
    </div>
</div>
