@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "BR Creator";
    Layout = "~/forms_standards/StandardLayout/NOAHEmpty/NOAHEmpty.cshtml";
}
<head>
    <script src="~/NOAH/DataSetup/NOAHBRCreator/NOAHBRCreator.js"></script>
    <link href="~/NOAH/DataSetup/NOAHBRCreator/NOAHBRCreator.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/NOAH/DataSetup/NOAHBRCreator/materials/fonts/new/fonts.css">
    <link rel="stylesheet" href="~/NOAH/DataSetup/NOAHBRCreator/materials/css/dashboard.css">
    <script src="~/NOAH/DataSetup/NOAHBRCreator/materials/js/dashboard.js"></script>

</head>

<div class="app-layout">

    <div class="app-hrd">

        <div class="app-hrd-tx">NOAH BR Creator</div>

        <div class="app-hrd-controls">

            <div class="app-btn-hide-sb"></div>

        </div>

    </div>

    <div class="app-sidebar">

        <div class="app-wrapper">

            <div class="app-btns">

                <div class="app-icons lib" data-content="lib" title="Libraries">
                    <span class="misc-curve"></span>
                </div>
                <div class="app-icons com" data-content="com" title="Component">
                    <span class="misc-curve"></span>
                </div>
                <div class="app-icons bot" data-content="bot" title="BRSS Components">
                    <span class="misc-curve"></span>
                </div>
                <div class="app-icons rig" data-content="rig" title="Top Components">
                    <span class="misc-curve"></span>
                </div>
                <div class="app-icons top" data-content="top" title="Bottom Components">
                    <span class="misc-curve"></span>
                </div>

            </div>


            <!-- original position of app-aside -->
        </div>



    </div>

    <div class="app-aside">
        <!--  --originally inside app-wrapper class -->

        <div class="app-aside-container" id="lib">
            <div class="app-aside-tx">Libraries & Assets</div>
            <div class="app-aside-stx">Drag & drop available widgets onto the form builder area to design your layout</div>

            <div class="app-dgdr-components">
                <div class="app-dgdr-items">
                    <div class="app-dgdr-item-tx">Layouts</div>
                    <div class="app-dgdr-item-components">
                        <div class="app-box"></div>
                        <div class="app-box"></div>
                        <div class="app-box"></div>
                    </div>
                </div>

                <div class="app-dgdr-items">
                    <div class="app-dgdr-item-tx">Widgets</div>
                    <div class="app-dgdr-item-components">
                        <div class="app-box"></div>
                        <div class="app-box"></div>
                        <div class="app-box"></div>
                        <div class="app-box"></div>
                        <div class="app-box"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="app-aside-container" id="com">
            <div class="app-aside-tx">v10 Components</div>
            <div id="brcomp" class="app-dgdr-components">
                <div class="app-dgdr-items">
                    <div class="app-dgdr-item-tx">Layouts</div>
                    <div class="app-dgdr-item-components">
                        <div class="app-box-list"></div>
                        <div class="app-box-list"></div>
                        <div class="app-box-list"></div>
                    </div>
                </div>

                <div class="app-dgdr-items">
                    <div class="app-dgdr-item-tx">Widgets</div>
                    <div class="app-dgdr-item-components">
                        <div class="app-box-list"></div>
                        <div class="app-box-list"></div>
                        <div class="app-box-list"></div>
                        <div class="app-box-list"></div>
                        <div class="app-box-list"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="app-aside-container" id="bot">
            <div class="app-aside-tx">BRSS Components</div>
        </div>
        <div class="app-aside-container" id="rig">
            <div class="app-aside-tx">Other Buttons Top</div>
        </div>
        <div class="app-aside-container" id="top">
            <div class="app-aside-tx">Bottom Components</div>
        </div>

    </div>

    <div class="app-main">
        <div class="app-dashboard">
            <div class="app-form "> @*menuitem-container*@
                <div class="container-flex">

                </div>
            </div>

        </div>
    </div>

</div>

