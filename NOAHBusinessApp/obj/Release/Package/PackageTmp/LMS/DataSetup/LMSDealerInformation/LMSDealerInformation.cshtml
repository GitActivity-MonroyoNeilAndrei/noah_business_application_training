                <div class="_icon"></div>
                <div class="_lbl">New</div>
            </div>
            <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-save noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Save</div>
            </div>
            <div id="noah-webui-default-Delete" class="btn-tb-action btn-tb-delete noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Delete</div>
            </div>
            <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Inquire</div>
            </div>
            <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh noah-webui-Toolbox-Item nwHide">
                <div class="_icon"></div>
                <div class="_lbl">Refresh</div>
            </div>
            <div id="noah-webui-default-Process" class="btn-tb-action btn-tb-process noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Process</div>
            </div>
            <div id="noah-webui-default-Print" class="btn-tb-action btn-tb-print noah-webui-Toolbox-Item nwHide">
                <div class="_icon"></div>
                <div class="_lbl">Print</div>
            </div>
            <div id="noah-webui-default-Export" class="btn-tb-action btn-tb-export noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Export</div>
            </div>

        </div>
    </div>

    <div class="container-flex">
        <div class="row row-parent padding-x-5 padding-y-5">
            <div class="col col-6 col-parent padding-x-5 padding-y-5">


                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Party Code</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtSuppliercode" type="text" class="txtbox" maxlength="30" />
                    </div>
                </div>

                <div class="row with-label-lookup">
                    <div class="col col-4">
                        <label class="lbltxt">Party Type<span class="nwRequiredField">*</span></label>
                    </div>
                    <div id="lugPartyType" class="col col-8 col-parent lookups">
