                </div>

                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Record Status</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtStatus" type="text" maxlength="30" />
                    </div>
                </div>



                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <div id="btnDisappRemarks" class="btn btn-default btn-default-blue btnMedium fullwidth" disabled="disabled">
                            Disapproval Details
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <div id="btnSalespersonAssign" class="btn btn-default btn-default-orange btnMedium fullwidth">
                            Salesperson Assignment
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <div id="btnReqCompliance" class="btn btn-default btn-default-orange btnMedium fullwidth">
                            Requirements Compliance
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <div id="btnViewContacts" class="btn btn-default btn-default-blue btnMedium fullwidth">
                            View Contracts
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <div id="btnViewUpdateHistory" class="btn btn-default btn-default-blue btnMedium fullwidth">
                            View Update History
                        </div>
                    </div>
                </div>
            </div>

        </div>


        <!--Component: Accordion -->
        <div class="row row-parent">
            <div class="col col-12 col-parent padding-x-5">
                <div class="row">
                    <div class="col col-12">
                        <div class="btn btn-default collapseExpand" id="btnCollapseAll">Expand All</div>
                    </div>
                </div>
                <div class="nk-accordion col col-12" nk-style-accordion="1" id="accordion_main">
                    <div class="nk-ul">
                        <div class="nk-li classHeader">
                            <div class="nk-li-title nwCuz001">Dealer Contact Information</div>
                            <div class="nk-li-content">
                                <div class="nk-li-text">
                                    <div class="col col-5">
                                    </div>
                                    <div class="row">
                                        <div class="col col-12">
                                            <div id="nwGrid1Con" class="col col-12 fullwidth">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="nk-li classHeader">
                            <div class="nk-li-title nwCuz001">Dealer Location Information</div>
                            <div class="nk-li-content">
                                <div class="nk-li-text">
                                    <div class="row row-parent child-centered">
                                        <div class="col col-6 col-parent">

                                            <div class="row">
                                                <div class="col col-4">
                                                    <label class="lbltxt">Preferred Mailing Address</label>
                                                </div>
                                                <div class="col col-8">
                                                    <select class="cmb fullwidth" id="cmbPrepMail"></select>

                                                </div>
                                            </div>


                                            <div class="row">
                                                <div class="col col-4">
                                                    <label class="lbltxt"><i><b><u>Present Address</u></b></i></label>
                                                </div>
                                                <div class="col col-8">
                                                </div>
                                            </div>                                         

                                            @*<div class="row">
                                                <div class="col col-4">
                                                    <label class="lbltxt">Location Code</label>
                                                </div>
                                                <div class="col col-8">
                                                    <input id="txtLocationCode" type="text" maxlength="30" />
                                                </div>
                                            </div>*@

                                            @*<div class="row with-label-lookup">
                                                <div class="col col-4">
                                                    <label class="lbltxt">Location Type<span class="nwRequiredField">*</span></label>
                                                </div>

                                                <div id="lugLocationType" class="col col-8 col-parent lookups lookupsCust">
                                                    <div class="col col-4">
