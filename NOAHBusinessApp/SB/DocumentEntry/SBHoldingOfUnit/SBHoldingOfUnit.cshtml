@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "Holding of Unit";
    Layout = NoahWebLib.nwSystem.GetDefaultLayout();
}

<head>
    <script src="~/SB/DocumentEntry/SBHoldingOfUnit/SBHoldingOfUnit.js"></script>
    <link href="~/SB/DocumentEntry/SBHoldingOfUnit/SBHoldingOfUnit.css" rel="stylesheet" />
</head>

@*ToolBox*@
<div id="noah-webui-Toolbox" class="nk-toolbar-c">
    <div id="noah-webui-default-New" class="btn-tb-action btn-tb-new noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">New</div>
    </div>
    <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-save noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Save</div>
    </div>
    <div id="noah-webui-default-Delete" class="btn-tb-action btn-tb-delete noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Delete</div>
    </div>
    <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Refresh</div>
    </div>
    <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Inquire</div>
    </div>
    <div id="noah-webui-default-Process" class="btn-tb-action btn-tb-process noah-webui-Toolbox-Item">
        <div class="_icon"></div>
        <div class="_lbl">Process</div>
    </div>
</div>

@*Main*@
<div class="container-flex">

    <div class="row row-parent padding-10">

        <div class="row">
            <div class="col col-12">
                <label class="lbltxt" style="color:#387ac5 !important; font-size:14px !important;font-weight: bold !important;">Note: Click PROCESS to submit the transaction for approval.</label>
            </div>
        </div>

      
        <div class="col col-6 col-parent">

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Customer Type</label>
                </div>
                <div class="col col-7">
                    <div class="col col-6">
                        <div class="rdo-wrapper">
                            <input id="rdIndividual" type="radio" name="rdCustomerType" class="rdo rdCustomerType" checked="checked">
                            <label class="lbl-rdo" for="rdIndividual">Individual</label>
                        </div>
                    </div>
                    <div class="col col-6">
                        <div class="rdo-wrapper">
                            <input id="rdCompany" type="radio" name="rdCustomerType" class="rdo rdCustomerType">
                            <label class="lbl-rdo" for="rdCompany">Company</label>
                        </div>
                    </div>
                </div>
            </div>
           
            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Customer Classification</label>
                </div>
                <div id="lugCustClassification" class="col col-7 col-parent lookups">
                    <div class="col col-5">
                        <input type="text" name="" class="txtbox idval clr">
                    </div>
                    <div class="col col-7">
                        <input type="text" name="" class="txtbox descval clr">
                        <div class="btn btn-sm-default btn-sm-default-lookup" id="btnCustClassification"></div>
                    </div>
                </div>
            </div>

            <div class="row with-label-lookup-single">
                <div class="col col-5">
                    <label class="lbltxt">Prospect Customer</label>
                </div>
                <div class="col col-7 col-parent lookups" id="lugProsCustomer">
                    <div class="col col-12" id="divConvalidvallugProsCustomer">
                        <input type="text" name="" class="txtbox idval txtlookup clr">
                        <div class="btn btn-sm-default btn-sm-default-lookup" id="btnProsCustomer"></div>
                    </div>
                </div>
            </div>

            <div class="row with-label-lookup-single">
                <div class="col col-5">
                    <label class="lbltxt">Customer</label>
                </div>
                <div class="col col-7 col-parent lookups" id="lugCustomer">
                    <div class="col col-12" id="divConvalidvallugCustomer">
                        <input type="text" name="" class="txtbox idval txtlookup clr">
                        <div class="btn btn-sm-default btn-sm-default-lookup" id="btnCustomer"></div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Last Name</label>
                </div>
                <div class="col col-7">
                    <input type="text" name="txtLastName" id="txtLastName" disabled="" class="input clr" maxlength="80">
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">First Name</label>
                </div>
                <div class="col col-7">
                    <input type="text" name="txtFirstName" id="txtFirstName" disabled="" class="input clr" maxlength="80">
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Middle Name</label>
                </div>
                <div class="col col-7">
                    <input type="text" name="txtMiddleName" id="txtMiddleName" disabled="" class="input clr" maxlength="80">
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Registered Name</label>
                </div>
                <div class="col col-7">
                    <textarea id="txtRegName" class="txtarea noresize txtAreaRemarks clr" rows="2" disabled="" style="max-height:50px !important; min-height:50px !important;"></textarea>
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Trade Name</label>
                </div>
                <div class="col col-7">
                    <textarea id="txtTradeName" class="txtarea noresize txtAreaRemarks clr" rows="2" disabled="" style="max-height:50px !important; min-height:50px !important;"></textarea>
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Record Status</label>
                </div>
                <div class="col col-7">
                    <input type="text" name="txtRecordStatus" id="txtRecordStatus" disabled="" class="input clr" maxlength="80">
                </div>
            </div>

        </div>

        <div class="col col-6 col-parent">
          
            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Location with Accountable Forms</label>
                </div>
                <div id="lugLocAcctForms" class="col col-7 col-parent lookups">
                    <div class="col col-5">
                        <input type="text" name="" class="txtbox idval clr">
                    </div>
                    <div class="col col-7">
                        <input type="text" name="" class="txtbox descval clr">
                        <div class="btn btn-sm-default btn-sm-default-lookup" id="btnAcctForm"></div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Transaction No.</label>
                </div>
                <div class="col col-7">
                    <input type="text" name="txtTransactionNo" id="txtTransactionNo" disabled="" class="input clr" maxlength="80">
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Type of Sale</label>
                </div>
                <div class="col col-7">
                    <input type="text" name="txtSaleType" id="txtSaleType" disabled="" class="input clr">
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Holding Type</label>
                </div>
                <div id="lugHoldingType" class="col col-7 col-parent lookups">
                    <div class="col col-5">
                        <input type="text" name="" class="txtbox idval clr">
                    </div>
                    <div class="col col-7">
                        <input type="text" name="" class="txtbox descval clr">
                        <div class="btn btn-sm-default btn-sm-default-lookup" id="btnHoldingType"></div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Holding Date</label>
                </div>
                <div class="col col-7">
                    <input type="date" name="txtHoldingDate" id="txtHoldingDate" class="txtbox txtbox-date input clr" disabled="">
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Holding Expiry Date</label>
                </div>
                <div class="col col-7">
                    <input type="date" name="txtHoldingExpiryDate" id="txtHoldingExpiryDate" class="txtbox txtbox-date input clr" disabled="">
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Reason for Return</label>
                </div>
                <div id="lugHoldingType" class="col col-7 col-parent lookups">
                    <div class="col col-5">
                        <input type="text" id="txtReasonForReturnCode" name="txtReasonForReturnCode" class="txtbox idval clr" disabled="">
                    </div>
                    <div class="col col-7">
                        <input type="text" id="txtReasonForReturnDesc" name="txtReasonForReturnDesc" class="txtbox descval clr" disabled="">
                        @*<div class="btn btn-sm-default btn-sm-default-lookup" id="btnHoldingType"></div>*@
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Remarks for Return</label>
                </div>
                <div class="col col-7">
                    <textarea id="txtRemarksForReturn" class="txtarea noresize txtAreaRemarks clr" rows="2" disabled="" style="max-height:40px !important; min-height:40px !important;"></textarea>
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                    <label class="lbltxt">Remarks</label>
                </div>
                <div class="col col-7">
                    <textarea id="txtRemarks" class="txtarea noresize txtAreaRemarks clr" rows="2" disabled="" style="min-height:60px !important; max-height:60px !important; "></textarea>
                </div>
            </div>

            <div class="row">
                <div class="col col-5">
                </div>
                <div class="col col-7">
                    <div id="btnReqCompliance" class="btn btn-default fullwidth">Requirements Compliance</div>
                </div>
            </div>

        </div>

    </div>

    <div class="row row-parent padding-10">
        <div class="col col-12" id="conUnitDtlsGrid" ></div>
    </div>
    

</div>

