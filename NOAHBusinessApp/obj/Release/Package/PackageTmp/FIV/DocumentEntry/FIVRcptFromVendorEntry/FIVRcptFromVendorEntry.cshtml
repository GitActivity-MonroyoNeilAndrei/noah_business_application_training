    <link href="~/FIV/DocumentEntry/FIVRcptFromVendorEntry/FIVRcptFromVendorEntry.css" rel="stylesheet" />
</head>
@*<body class="">*@

@*TOOLBOX*@

<div class="nk-toolbar-c">
    <div id="noah-webui-Toolbox" class="nk-toolbar-c noah-webui-Toolbox">
        <div class="nk-toolbar-btn noah-webui-Toolbox">
            <div id="noah-webui-default-New" class="btn-tb-action btn-tb-new noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">New</div>
            </div>
            <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-save noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Save</div>
            </div>
            <div id="noah-webui-default-Delete" class="btn-tb-action btn-tb-delete noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Delete</div>
            </div>
            <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Refresh</div>
            </div>
            <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Inquire</div>
            </div>
            <div id="noah-webui-default-Process" class="btn-tb-action btn-tb-process noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Process</div>
            </div>
            <div id="noah-webui-default-Print" class="btn-tb-action btn-tb-print noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Print</div>
            </div>           
        </div>
    </div>
</div>


<div class="nk-container-flex padding-10">
    <div class="row row-parent">
        <div class="col col-6 col-parent">
            <input id="tagChkRcptRefNoConfig" class="nwHide" />
            <input id="txtServerdate" class="nwHide" />
            <input id="txtChkAllowBackdate" class="nwHide" />

            @*Purchase Order No.*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Purchase Order No.<span class="nwRequiredField">*</span></label>
                </div>
                <div id="lugPurchaseOrderNo" class="col col-8 col-parent lookups">
                    <div class="col col-8">
                        <input type="text" name="" class="txtbox idval" />
                    </div>
                    <div class="col col-4">
                        <input type="text" name="" class="txtbox descval" />
                        <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                    </div>
                </div>
            </div>



            @*Ref. No. (BS/SI/SOA)*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Ref. No. (BS/SI/SOA)<span class="nwOptionalField">*</span></label>
                </div>
                <div class="col col-8">
                    <input id="txtSINo" type="text" maxlength="30"
                           name=""
                           class="idval" />
                </div>
            </div>
            @*Ref. Date*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Ref. Date<span class="nwOptionalField">*</span></label>
                </div>
                <div class="col col-8">
                    <input id="dtpSIDate" class="txtbox clrtxt nwDatePick" type="text" disabled="" autocomplete="off">
                </div>
            </div>


            @*Counter Date*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Counter Date</label>
                </div>
                <div class="col col-8">
                    <input id="txtCounterDate" class="txtbox clrtxt nwDatePick" type="text" disabled="" autocomplete="off">
                </div>
            </div>
            @*DR/COC No.*@

            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">DR/COC No.<span class="nwOptionalField">*</span></label>
                </div>
                <div class="col col-8">

                    <input id="txtDRNo" type="text" maxlength="30"
                           name=""
                           class="idval" />
                </div>
            </div>
            @*DR/COC Date*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">DR/COC Date<span class="nwOptionalField">*</span></label>
                </div>
                <div class="col col-8">
                    <input id="dtpDRDate" class="txtbox clrtxt nwDatePick" type="text" disabled="" autocomplete="off">
                </div>
            </div>

            @*REMARKS*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Remarks<span class="nwRequiredField">*</span></label>
                </div>
                <div class="col col-8">
                    <textarea id="txtParticular" maxlength="500" class="txtarea autoresize" rows="3"></textarea>
                </div>
            </div>

            @*Value Date*@
            <div class="row" id="tagDocdate">
                <div class="col col-4">
                    <label class="lbltxt">Value Date<span class="nwRequiredField">*</span></label>
                </div>
                <div class="col col-8">
                    <input id="txtDocDate" class="txtbox clrtxt nwDatePick" type="text" disabled="" autocomplete="off">
                </div>
            </div>

            @*BUTTONS*@

            <div class="row">
                <div class="col col-4"></div>
                <div class="col col-8">
                    <div id="btnViewAttachment" class="btn btn-default fullwidth">Review Attachment(s)</div>



                </div>
            </div>

            <div class="row">
                <div class="col col-4"></div>
                <div class="col col-8">
                    <div id="btnReqCompliance" class="btn btn-default btn-default-orange fullwidth">Requirement Compliance</div>


                </div>
            </div>


        </div>





        <div class="col col-6 col-parent">
            @*TRANSACTION NUMBER*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Transaction No.</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtTransactionNo" name="" class="txtbox" disabled>
                </div>
            </div>
            @*Date Submitted*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Date Submitted</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtSubmittedDate" name="" class="txtbox" disabled>
                </div>
            </div>
            @*Date Posted*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Date Posted</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtDatePosted" name="" class="txtbox" disabled>
                </div>
            </div>
            @*PO Location*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">PO Location</label>
                </div>

                <div id="lugLocForm" class="col col-8 col-parent lookups lookupsCust">
                    <div class="col col-4">
                        <input type="text" name="" class="txtbox idval" disabled>
                    </div>
                    <div class="col col-8">
                        <input type="text" name="" class="txtbox txtlookup descval" disabled>
                    </div>
                </div>
            </div>

            @*Delivery Location*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Delivery Location</label>
                </div>
                <div id="lugDelLocForm" class="col col-8 col-parent lookups lookupsCust">
                    <div class="col col-4">
                        <input type="text" name="" class="txtbox idval" disabled>
                    </div>
                    <div class="col col-8">
                        <input type="text" name="" class="txtbox txtlookup descval" disabled>
                    </div>
                </div>
            </div>

            @*Delivery Sublocation*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Delivery Sublocation</label>
                </div>
                <div id="lugSubLoc1" class="col col-8 col-parent lookups lookupsCust">
                    <div class="col col-4">
                        <input type="text" name="" class="txtbox idval" disabled>
                    </div>
                    <div class="col col-8">
                        <input type="text" name="" class="txtbox txtlookup descval" disabled>
                    </div>
                </div>
            </div>

            @*Cost Center*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Cost Center</label>
                </div>
                <div id="lugCostCenter" class="col col-8 col-parent lookups lookupsCust">
                    <div class="col col-4">
                        <input type="text" name="" class="txtbox idval" disabled>
                    </div>
                    <div class="col col-8">
                        <input type="text" name="" class="txtbox txtlookup descval" disabled>
                    </div>
                </div>
            </div>

            @*Vendor*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Vendor</label>
                </div>
                <div id="lugVendor" class="col col-8 col-parent lookups lookupsCust">
                    <div class="col col-4">
                        <input type="text" name="" class="txtbox idval" disabled>
                    </div>
                    <div class="col col-8">
                        <input type="text" name="" class="txtbox txtlookup descval" disabled>
                    </div>
                </div>
            </div>

            @*Item Type*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Item Type</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtItemType" name="" maxlength="80" class="txtbox" disabled>
                </div>
            </div>

            @*Total Qty*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Total Qty</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtTotalQty" name="" maxlength="80" disabled>
                </div>
            </div>

            @*DOCUMENT STATUS*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Document Status</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtStatus" name="" class="txtbox" disabled>
                </div>
            </div>
            @*REASON FOR DISAPPROVAL*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Reason for Disapproval</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtReasonDisapp" name="" class="txtbox" disabled>
                </div>
            </div>
            @*REMARKS FOR DISAPPROVAL*@
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Disapproval Remarks</label>
                </div>
                <div class="col col-8">
                    <textarea id="txtDisappRemarks" class="txtarea autoresize" maxlength="500" rows="3" disabled></textarea>
                </div>
            </div>

            @*Button*@

            <div class="row">
                <div class="col col-4"></div>
                <div class="col col-8">
                    <div id="btnViewJournal" class="btn btn-default fullwidth">View Journal Entries</div>
                    @*<div class="col col-8">*@
                    @*<div id="btnViewAttachment" class="btn btn-default">Review Attachment(s)</div>*@


                </div>
            </div>

        </div>
    </div>
</div>

