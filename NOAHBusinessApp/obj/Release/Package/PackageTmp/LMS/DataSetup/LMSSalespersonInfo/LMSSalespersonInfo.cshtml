                    </div>
                    <div class="col col-1">
                        <input id="txtStreetNo" class="" type="text" maxlength="12" />
                    </div>

                    <div class="col col-0-25">
                    </div>
                    <div class="col col-2-50">
                        <label class="lbltxt">Street Name<span class="nwOptionalField opt"> *</span></label>
                    </div>
                    <div class="col col-4-25">
                        <input id="txtStreetName" class="" type="text" maxlength="80" />
                    </div>
                </div>

                @*Lot*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Lot</label>
                    </div>
                    <div class="col col-1">
                        <input id="txtLot" class="" type="text" maxlength="25" />
                    </div>

                    <div class="col col-0-25">
                    </div>
                    <div class="col col-1-75">
                        <label class="lbltxt">Block</label>
                    </div>
                    <div class="col col-1-75">
                        <input id="txtBlock" class="" type="text" maxlength="25" />
                    </div>

                    <div class="col col-0-25">
                    </div>
                    <div class="col col-1-75">
                        <label class="lbltxt">Phase</label>
                    </div>
                    <div class="col col-1-25">
                        <input id="txtPhase" class="" type="text" maxlength="25" />
