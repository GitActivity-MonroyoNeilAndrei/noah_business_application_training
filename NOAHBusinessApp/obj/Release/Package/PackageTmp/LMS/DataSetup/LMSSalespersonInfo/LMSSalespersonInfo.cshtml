                    <div class="col col-4">
                        <label class="lbltxt">Province</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtProvinceCode" class="nwHide" type="text" />
                        <input id="txtProvince" class="" type="text" />
                    </div>
                </div>

                @*Region*@
                <div class="row">
                    <div class="col col-4">
