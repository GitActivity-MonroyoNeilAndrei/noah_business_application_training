@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "NOAHIssueLogs";
    Layout = "~/forms_standards/StandardLayout/NOAHSpreadDesigner/NOAHSpreadDesigner.cshtml";
}
<head>
    <script src="~/NOAH/DocumentEntry/NOAHIssueLogs/NOAHIssueLogs.js"></script>
    <link href="~/NOAH/DocumentEntry/NOAHIssueLogs/NOAHIssueLogs.css" rel="stylesheet" />
