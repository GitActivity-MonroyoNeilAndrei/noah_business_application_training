@inherits System.Web.Mvc.WebViewPage
@{
     ViewBag.Title = "Deactivated Client Registration Summary";
     Layout =NoahWebLib.nwSystem.GetDefaultLayout();
}

<head>
    <script src="~/SB/ReportsAndInquiry/SBDeactivatedClientRegSummRpt/SBDeactivatedClientRegSummRpt.js"></script>
    <link href="~/SB/ReportsAndInquiry/SBDeactivatedClientRegSummRpt/SBDeactivatedClientRegSummRpt.css" rel="stylesheet" />
</head>

<div id="noah-webui-Toolbox" class="nk-toolbar-c">
        <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh">
            <div class="_icon"></div>
            <div class="_lbl">Refresh</div>
        </div>
        <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire">
            <div class="_icon"></div>
            <div class="_lbl">Inquire</div>
        </div>
</div>

<div class="container-flex">
    <div class="row row-parent padding-10">
        <!-- Date Filter -->
        <div class="col col-4">
            <fieldset class="fieldset" nk-fieldset-style="6">
                <legend>Date Filter</legend>
                <div class="row">
                    <div class="col col-12 col-parent">
                        <div class="row">
                            <div class="col col-4">
                                <input id="rdMonthly" type="radio" name="dateFilter" class="rdo rdDateFilter" filtername="monthly" />
                                <label class="lbl-rdo" for="rdMonthly">Monthly</label>
                            </div>
                            <div class="col col-8">
                                <select id="dpMonthly" class="cmb fullwidth inputFilter" disabled="disabled">
                                </select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col col-4">
                                <input id="rdAnnually" type="radio" name="dateFilter" class="rdo rdDateFilter" filtername="annually" checked />
                                <label class="lbl-rdo" for="rdAnnually">Annually</label>
                            </div>
                            <div class="col col-8">
                                <select id="dpAnnually" class="cmb fullwidth inputFilter">
                                </select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col col-4">
                                <input id="rdQuarterly" type="radio" name="dateFilter" class="rdo rdDateFilter" filtername="quarterly" />
                                <label class="lbl-rdo" for="rdQuarterly">Quarterly</label>
                            </div>
                            <div class="col col-8">
                                <select id="dpQuarterly" class="cmb fullwidth inputFilter" disabled="disabled">
                                </select>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col col-12">
                                <input id="rdDateCovered" type="radio" name="dateFilter" class="rdo rdDateFilter" filtername="datecreated" />
                                <label class="lbl-rdo" for="rdDateCovered">Date Covered</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col col-1">
                                
                            </div>
                            <div class="col col-3">
                                <label class="lbltxt" for="txtFrom">From</label>
                            </div>
                            <div class="col col-8">
                                <input id="txtFrom" type="text" name="" class="txtbox inputFilter txtDate txtbox-date isNumber nwDatePick" disabled="disabled" />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col col-1">

                            </div>
                            <div class="col col-3">
                                <label class="lbltxt" for="txtTo">To</label>
                            </div>
                            <div class="col col-8">
                                <input id="txtTo" type="text" name="" class="txtbox inputFilter txtDate txtbox-date isNumber nwDatePick" disabled="disabled" />
                            </div>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
        <!-- Filter by -->
        <div class="col col-8 col-parent nwRightheader">
            <fieldset class="fieldset fullwidth" nk-fieldset-style="6">
                <legend>Filter By</legend>
                <div class="row">
                    <div class="col col-12">
                        <div class="tabs tabs-lbl-bold fullwidth">
                            <div id="settingstabs" class="tabs-content">

                            </div>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
        <div class="col col-1">
            
        </div>
    </div>
</div>

<div class="container-flex">
    <div class="row row-parent padding-10">
        <div class="col col-12" id="conGrid">
        </div>
    </div>
</div>


