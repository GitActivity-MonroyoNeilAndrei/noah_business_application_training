@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "Document Attachment";
}


<div id="docattview" nwTitle="Document Attachment">

    <div id="noah-webui-Toolbox" class="nk-toolbar-c">
        <div class="nk-toolbar-btn">
            <div id="docattviewdownload" class="btn-tb-action downloadbuttoncss">
                <div class="_icon"></div>
                <div class="_lbl">Download</div>
            </div>
            <div id="docattviewzoomin" class="btn-tb-action zoominbuttoncss">
                <div class="_icon"></div>
                <div class="_lbl">Zoom-In</div>
            </div>
            <div id="docattviewzoomout" class="btn-tb-action zoomoutbuttoncss">
                <div class="_icon"></div>
                <div class="_lbl">Zoom-Out</div>
            </div>
            <a id="aDownload" target="_blank" href="#"></a>

        </div>
    </div>

    <div class="container-flex">

        <div class="wrapper">
            <div id="vwImage" class="h_iframe">
                <!-- a transparent image is preferable -->
                <img id="docattviewimg" class="ratio" src="" />
                <iframe id="docattviewlink" src="" frameborder="0" allowfullscreen></iframe>
            </div>
            <object id="docattviewpdfobjectlink" data="" type="application/pdf">
                <embed id="docattviewpdfembedlink" src="" type="application/pdf" width="90%" height="90%" />
            </object>

        </div>
