<div class="container-flex">
    <div class="row row-parent padding-10">
        <!-- Left Main Area Start -->
        <div class="col col-6">
            <!-- Require Message -->
            <div class="row">
                <label class="lbltxt nwRequiredField">Please specify all the required (*) fields.</label>
            </div>
            <!-- Space -->
            <div class="row">
                <label class="lbltxt"> </label>
            </div>
            <div class="row">
                <label class="lbltxt"> </label>
            </div>
            <!-- Seller -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Seller</label>
                </div>
                <div id="lugSeller" class="col col-8 lookups">
                    <div class="col col-4 conval">
                        <input type="text" name="" class="txtbox txtcode idval" autocomplete="off">
                    </div>
                    <div class="col col-8 conval">
                        <input type="text" name="" class="txtbox txtdesc descval" autocomplete="off">
                    </div>
                    <input type="hidden" id="txtTransactionNo" value="" class="nwHideImportant" disabled />
                </div>
            </div>
            <!-- Prospect Customer -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Prospect Customer</label>
                </div>
                <div id="lugProspectCust" class="col col-8 lookups">
                    <div class="col col-4 conval">
                        <input type="text" name="" class="txtbox txtcode idval" autocomplete="off">
                    </div>
                    <div class="col col-8 conval">
                        <input type="text" name="" class="txtbox txtdesc txtlookup descval" autocomplete="off">
                        <div class="btn btn-sm-default btn-sm-default-lookup LookUp"></div>
                    </div>
                </div>
            </div>
            <!-- Sales Force Activity -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Sales Force Activity</label>
                </div>
                <div id="lugSalesForceAct" class="col col-8 lookups">
                    <div class="col col-4 conval">
                        <input type="text" name="" class="txtbox txtcode idval" autocomplete="off">
                    </div>
                    <div class="col col-8 conval">
                        <input type="text" name="" class="txtbox txtdesc txtlookup descval" autocomplete="off">
                        <div class="btn btn-sm-default btn-sm-default-lookup LookUp"></div>
                    </div>
                </div>
            </div>
            <!-- Location -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Location</label>
                </div>
                <div id="lugLoc" class="col col-8 lookups">
                    <div class="col col-4 conval">
                        <input type="text" name="" class="txtbox txtcode idval" autocomplete="off">
                    </div>
                    <div class="col col-8 conval">
                        <input type="text" name="" class="txtbox txtdesc txtlookup descval" autocomplete="off">
                        <div class="btn btn-sm-default btn-sm-default-lookup LookUp"></div>
                    </div>
                </div>
            </div>
            <!-- Project -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Project</label>
                </div>
                <div id="lugProj" class="col col-8 lookups">
                    <div class="col col-4 conval">
                        <input type="text" name="" class="txtbox txtcode idval" autocomplete="off">
                    </div>
                    <div class="col col-8 conval">
                        <input type="text" name="" class="txtbox txtdesc txtlookup descval" autocomplete="off">
                        <div class="btn btn-sm-default btn-sm-default-lookup LookUp"></div>
                    </div>
                </div>
            </div>
            <!-- Tower/Phase -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Tower/Phase</label>
                </div>
                <div id="lugTowerPhase" class="col col-8 lookups">
                    <div class="col col-4 conval">
                        <input type="text" name="" class="txtbox txtcode idval" autocomplete="off">
                    </div>
                    <div class="col col-8 conval">
                        <input type="text" name="" class="txtbox txtdesc txtlookup descval" autocomplete="off">
                        <div class="btn btn-sm-default btn-sm-default-lookup LookUp"></div>
                    </div>
                </div>
            </div>
            <!-- Mobile Number -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Mobile Number</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtMobileNo" name="" class="txtbox txtcode isNumber" maxlength="80" autocomplete="off">
                </div>
            </div>
            <!-- Email Address -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Email Address</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtEmailAdd" name="" class="txtbox txtcode" maxlength="80" autocomplete="off">
                </div>
            </div>
            <!-- Start Date/Time -->
            <div class="row">
                <div class="col col-4">
                    <input type="hidden" id="txtServerDate" name="" class="txtbox txtcode nwHideImportant" disabled autocomplete="off">
                    <label class="lbltxt">Start Date/Time</label>
                </div>
                <div class="col col-8">
                    <input type="hidden" id="txtOldStartDate" name="" class="txtbox txtcode nwHideImportant" disabled autocomplete="off">
                    <input type="datetime-local" id="txtStartDate" name="" class="txtbox txtcode isNumber" autocomplete="off">
                </div>
            </div>
            <!-- End Date/Time -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">End Date/Time</label>
                </div>
                <div class="col col-8">
                    <input type="hidden" id="txtOldEndDate" name="" class="txtbox txtcode nwHideImportant" disabled autocomplete="off">
                    <input type="datetime-local" id="txtEndDate" name="" class="txtbox txtcode isNumber" autocomplete="off">
                </div>
            </div>
            <!-- Notification Schedule -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Notification Schedule</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtNotifSched" name="" class="txtbox txtcode isNumber" placeholder="minutes before the start date time" maxlength="12" autocomplete="off">
                </div>
            </div>
            <!-- Notification Manner -->
            <div class="row">
                <div class="col col-4">
                    <input type="hidden" id="ComboBox1" name="" class="txtbox txtcode nwHideImportant" disabled autocomplete="off">
                    <label class="lbltxt">Notification Manner</label>
                </div>
                    
                <div class="col col-8 col-parent">
                    <div class="col col-3">
                        <div class="chk-con">
                            <input id="chkSMS" type="checkbox" name="" class="chk">
                            <label class="lbl-chk" for="chkSMS">SMS</label>
                        </div>
                    </div>
                    <div class="col col-3">
                        <div class="chk-wrapper">
                            <input id="chkEmail" type="checkbox" name="" class="chk">
                            <label class="lbl-chk" for="chkEmail">Email</label>
                        </div>
                    </div>
                    <div class="col col-6">
                        
                    </div>
                </div>
            </div>
            <!-- Who will receive? -->
            <div class="row">
                <div class="col col-4">
                    <input type="hidden" id="ComboBox2" name="" class="txtbox txtcode nwHideImportant" disabled autocomplete="off">
                    <label class="lbltxt">Who will receive?</label>
                </div>
                <div class="col col-8 col-parent">
                    <div class="col col-3">
                        <div class="chk-con">
                            <input id="chkSeller" type="checkbox" name="" class="chk">
                            <label class="lbl-chk" for="chkSeller">Seller</label>
                        </div>
                    </div>
                    <div class="col col-3">
                        <div class="chk-con">
                            <input id="chkBuyer" type="checkbox" name="" class="chk">
                            <label class="lbl-chk" for="chkBuyer">Buyer</label>
                        </div>
                    </div>
                    <div class="col col-6">
                        <div class="chk-con">
                            <input id="chkImmediateSuperior" type="checkbox" name="" class="chk">
                            <label class="lbl-chk" for="chkImmediateSuperior">Immediate Superior</label>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Remarks -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Remarks</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtRemarks" name="" class="txtbox txtcode" maxlength="500" autocomplete="off">
                </div>
            </div>
            <!-- Record Status -->
            <div class="row">
                <div class="col col-4">
                    <label class="lbltxt">Record Status</label>
                </div>
                <div class="col col-8">
                    <input type="text" id="txtRecordStat" name="" class="txtbox txtcode" maxlength="80" autocomplete="off">
                </div>
            </div>
            <!-- Last Details -->
            <div class="row">
                <div class="col col-4">
                    <input type="hidden" id="radiolists" name="" class="txtbox txtcode nwHideImportant" disabled autocomplete="off">
                    <label class="lbltxt"> </label>
                </div>
                <div class="col col-8 col-parent">
                    <div class="col col-3">
                        <div class="rdo-wrapper">
                            <input id="rdbAttended" type="radio" name="rdbRecStat" class="rdo">
                            <label class="lbl-rdo" for="rdbAttended">Attended</label>
                        </div>
                    </div>
                    <div class="col col-3">
                        <div class="rdo-wrapper">
                            <input type="hidden" id="forReSched" name="" class="txtbox txtcode nwHideImportant" disabled autocomplete="off">
                            <input id="rdbResched" type="radio" name="rdbRecStat" class="rdo">
                            <label class="lbl-rdo" for="rdbResched">Reschedule</label>
                        </div>
                    </div>
                    <div class="col col-3">
                        <div class="rdo-wrapper">
                            <input id="rdbCancel" type="radio" name="rdbRecStat" class="rdo">
                            <label class="lbl-rdo" for="rdbCancel">Cancel</label>
                        </div>
                    </div>
                    <div class="col col-3">
                        <div id="btnViewRecords" class="color-picker-btn color-picker-c" >Show All Records</div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Left Main Area End -->

        <!-- Right Main Area Start -->
        <div class="col col-6">

        </div>
        <!-- Right Main Area End -->
    </div>
</div>

<div class="container-flex">
    <div class="col col-12">
        <div id="SalesCallSumm" class="row row-parent padding-10 nwCuz001" title="Sales Call Summary">
            <iframe id="Details" class="nwCuz002 col-12"></iframe>
        </div>
    </div>
</div>

