<div class="container-flex padding-10">
    <div class="row">
        <div class="col col-4 col-parent">
            <div class="row with-label-lookup">
                <div class="col col-3">
                    <label class="lbltxt">Company</label>
                </div>
                <div class="col col-9 col-parent">
                    <div class="col col-5">
                        <input type="text" name="" class="txtbox txtcode" id="idvallugCode">
                    </div>
                    <div class="col col-7">
                        <input type="text" name="" class="txtbox txtdesc txtlookup" id="descvallugCode">
                        <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                    </div>
                </div>
            </div>

            <div class="row with-label-lookup">
                <div class="col col-3">
                    <label class="lbltxt">Username</label>
                </div>
                <div class="col col-9 col-parent">
                    <div class="col col-5">
                        <input type="text" name="" class="txtbox txtcode" id="idvallugCode2">
                    </div>
                    <div class="col col-7">
                        <input type="text" name="" class="txtbox txtdesc txtlookup" id="descvallugCode2">
                        <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col col-8 col-parent"></div>
    </div>

    <div class="row margin-top-15">
        <div class="col col-4 col-parent">
            <div class="row with-label-lookup">
                <div class="col col-12 col-parent">
                    <div class="col col-6">
                        <select class="cmb fullwidth" id="combobox1">
                            <option>Code</option>
                            <option>Description</option>
                        </select>
                    </div>
                    <div class="col col-6">
                        <input type="text" name="" class="txtbox" id="inSearch">
                    </div>
                </div>
            </div>
        </div>
        <div class="col col-8 col-parent">
            <div class="row">
                <div class="col col-12 col-parent">
                    <div class="col col-2">
                        <div class="btn btn-default fullwidth">Load User Alert</div>
                    </div>
                    <div class="col col-2">
                        <div class="btn btn-default fullwidth">Save User Alert</div>
                    </div>
                    <div class="col col-2">
                        <div class="btn btn-default fullwidth">Copy From User</div>
                    </div>
                    <div class="col col-6"></div>
                </div>
            </div>
        </div>
    </div>
</div>
