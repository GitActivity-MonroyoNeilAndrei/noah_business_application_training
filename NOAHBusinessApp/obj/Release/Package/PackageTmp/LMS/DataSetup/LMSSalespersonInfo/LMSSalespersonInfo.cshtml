                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>

                @*Allow Multiple Dealer*@
                <div class="row">
                    <div class="col col-4">
                    </div>
                    <div class="col col-8">
                        <div class="chk-all-con">
                            <input type="checkbox" id="chkisMultilpleDealer" class="chk-all-s unchecked" check-all-for="disapprove">
                            <label class="lbl-chkall" for="chkisMultilpleDealer">Allow Multiple Dealer</label>
                        </div>
                    </div>
                </div>

                @*Tax Identification No.*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Tax Identification No.</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtTaxIDNo" class="nwRightAlignText" type="text" minlength="9" maxlength="12" />
                    </div>
                </div>

                @*SSS No.*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">SSS No.</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtSSSNo" class="nwRightAlignText" type="text" />
                    </div>
                </div>

                @*Other ID*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Other ID</label>
                    </div>
                    <div class="col col-8 col-parent lookups" id="lugOtherID">
