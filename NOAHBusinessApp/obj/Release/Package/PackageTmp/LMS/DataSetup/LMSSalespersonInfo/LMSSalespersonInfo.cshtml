                    </div>
                    <div class="col col-8 col-parent lookups" id="lugCivilStats">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>

                @*Job Title*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Job Title<span class="nwRequiredField"> *</span></label>
                    </div>
                    <div class="col col-8 col-parent lookups" id="lugJobTitle">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>

                @*Relationship to Dealer*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Relationship to Dealer<span class="nwRequiredField"> *</span></label>
                    </div>
                    <div class="col col-8 col-parent lookups" id="lugRelationToDealer">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8">
