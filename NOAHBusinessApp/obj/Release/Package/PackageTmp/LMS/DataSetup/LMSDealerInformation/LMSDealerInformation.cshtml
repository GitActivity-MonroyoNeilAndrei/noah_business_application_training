                    <div class="col col-4">
                        <label class="lbltxt">BIR Registration Type</label>
                    </div>
                    <div class="col col-8">
                        <div class="row">
                            <div class="col col-4">
                                <div class="rdo-wrapper">
                                    <input id="rbVat"
                                           type="radio"
                                           name="sample1"
                                           class="rdo" />
                                    <label class="lbl-rdo" for="rbVat">VAT</label>
                                </div>
                            </div>
                            <div class="col col-4">
                                <div class="rdo-wrapper">
                                    <input id="rbNonVat"
                                           type="radio"
                                           name="sample1"
                                           class="rdo" />
                                    <label class="lbl-rdo" for="rbNonVat">Non-VAT</label>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>

