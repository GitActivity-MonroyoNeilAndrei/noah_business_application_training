@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "Billing History";
    Layout =NoahWebLib.nwSystem.GetDefaultLayout();
}

<head>
    <script src="~/PMO/ReportsAndInquiry/PMOBillingHistory/PMOBillingHistory.js"></script>
    <link href="~/PMO/ReportsAndInquiry/PMOBillingHistory/PMOBillingHistory.css" rel="stylesheet" />
</head>



<div class="container-flex padding-5">


    <div class="row">
        <div class="col col-12">
            <div class="menuitem-title">Billing History</div>
        </div>

        <div class="col col-12">
            <div class="menuitem-df">Date Filter</div>
        </div>
    </div>


    <div class="row">
        <div class="col col-4 margin-x-0">
            <div class="row margin-bottom-5">
                <div class="col col-2">
                    <label class="lbltxt">From</label>
                </div>
                <div class="col col-10">
                    <input type="text" name="" class="txtbox nwDatePicker" id="dateFilterFrom" placeholder="mm/dd/yyyy">
                </div>
            </div>
            <div class="row margin-bottom-5">
                <div class="col col-2">
                    <label class="lbltxt">To</label>
                </div>
                <div class="col col-10">
                    <input type="text" name="" class="txtbox nwDatePicker" id="dateFilterTo" placeholder="mm/dd/yyyy">
                </div>
            </div>
            <div class="row margin-bottom-5">
                <div class="col col-2">
                    <label class="lbltxt">Status</label>
                </div>
                <div class="col col-10">
                    <select class="cmb fullwidth" id="statusFilter">
                        <option>Select All</option>
                        <option>Paid</option>
                        <option>Unpaid</option>
                    </select>
                </div>
            </div>
        </div>
    </div>






    <div class="row">
        <div class="col col-12">
            <div id="conGrid" class="nwCuz004">
            </div>
        </div>
    </div>



</div>



