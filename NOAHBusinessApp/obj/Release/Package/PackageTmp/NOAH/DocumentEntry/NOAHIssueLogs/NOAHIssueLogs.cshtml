                    <div class="row">
                        <div class="col col-4">
                            <div class="btn btn-default fullwidth" id="btnnwgRemarksSave">Save and Exit</div>
                        </div>
                        <div class="col col-8">
                            <div class="chk-con">
                                <input id="chknwgRemarks" type="checkbox" name="sample1" class="chk">
                                <label class="lbl-chk" for="chk-htr">Auto Close upon enter</label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div id="cmsnwgRemarks" class="nw-htr-fx">

            </div>
        </div>
    </div>
</div>
@*end html remarks*@




