    <div class="nk-container-flex padding-10">
        <div class="row row-parent">

            @*LEFT*@
            <div class="col col-3-75 col-parent">

                @*Salesperson Code*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Salesperson Code</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtSalesPersonCode" class="nwUpper nwCode nwNoSpace" type="text" maxlength="12" />
                    </div>
                </div>

                @*Salesperson Name*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Salesperson Name</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtSalesPersonName" class="" type="text" />
                    </div>
                </div>

                @*Cross Reference Code*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Cross Reference Code</label>
                    </div>
                    <div class="col col-8">
                        <input id="txtCrossRefCode" class="nwUpper nwCode nwNoSpace" type="text" maxlength="30" />
                    </div>
                </div>

                <br /><br /><br /><br />

                @*Party Type*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Party Type<span class="nwRequiredField"> *</span></label>
                    </div>
                    <div class="col col-8 col-parent lookups" id="lugPartyType">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>

                @*Salutation*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Salutation</label>
                    </div>
                    <div class="col col-8 col-parent lookups" id="lugSalutation">
                        <div class="col col-4">
                            <input type="text" name="" class="txtbox idval">
                        </div>
                        <div class="col col-8">
                            <input type="text" name="" class="txtbox descval">
                            <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                        </div>
                    </div>
                </div>

                @*Last Name*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Last Name<span class="nwRequiredField"> *</span></label>
                    </div>
                    <div class="col col-8">
                        <input id="txtLastName" class="" type="text" maxlength="80" />
                    </div>
                </div>

                @*First Name*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">First Name<span class="nwRequiredField"> *</span></label>
                    </div>
                    <div class="col col-8">
                        <input id="txtFirstName" class="" type="text" maxlength="80" />
                    </div>
                </div>

                @*Middle Name*@
                <div class="row">
                    <div class="col col-4">
                        <label class="lbltxt">Middle Name</label>
                    </div>
                    <div class="col col-5">
                        <input id="txtMiddleName" class="" type="text" maxlength="80" />
                    </div>
                    <div class="col col-1">
                    </div>
                    <div class="col col-1">
                        <label class="lbltxt">M.I.</label>
                    </div>
                    <div class="col col-1">
                        <input id="txtMI" class="" type="text" maxlength="5" />
