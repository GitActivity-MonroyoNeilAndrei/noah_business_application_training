                    <div class="col col-3">
                        <label class="lbltxt">Document No.</label>
                    </div>
                    <div class="col col-5">
                        <input type="text" name="" class="txtbox" id="txtDocno"/>
                        <input type="text" name="" class="txtbox nwHide" id="txtServerdate" />
                        <input type="text" name="" class="txtbox nwHide" id="txtRecuser" />
                    </div>
                    <div class="col col-4">
                    </div>
                </div>

                @*Date Submitted*@
                <div class="row">
                    <div class="col col-3">
                        <label class="lbltxt">Date Submitted</label>
                    </div>
                    <div class="col col-5">
                        <input type="text" name="" class="txtbox" id="dtpDateSubmit"/>
                    </div>
                    <div class="col col-4">
                    </div>
                </div>

