@inherits System.Web.Mvc.WebViewPage			
@{			
    ViewBag.Title = "Utilities Billing Entry";
    Layout = NoahWebLib.nwSystem.GetDefaultLayout();
}			
<head>
    <script src="~/PMO/DocumentEntry/PMOUtilitiesBillingEntry/PMOUtilitiesBillingEntry.js"></script>
    <script src="~/PMO/DocumentEntry/PMOUtilitiesBillingEntry/UploadCustom.js"></script>
    <link href="~/PMO/DocumentEntry/PMOUtilitiesBillingEntry/PMOUtilitiesBillingEntry.css" rel="stylesheet" />
</head>		
<body>
    <div id="noah-webui-Toolbox" class="nk-toolbar-c noah-webui-Toolbox">
        <div class="nk-toolbar-btn noah-webui-Toolbox">
            <div id="noah-webui-default-New" class="btn-tb-action btn-tb-new noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">New</div>
            </div>
            <div id="noah-webui-default-Save" class="btn-tb-action btn-tb-save noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Save</div>
            </div>
            <div id="noah-webui-default-Delete" class="btn-tb-action btn-tb-delete noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Delete</div>
            </div>
            <div id="noah-webui-default-Refresh" class="btn-tb-action btn-tb-refresh noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Refresh</div>
            </div>
            <div id="noah-webui-default-Inquire" class="btn-tb-action btn-tb-inquire noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Inquire</div>
            </div>
            <div id="noah-webui-default-Process" class="btn-tb-action btn-tb-process noah-webui-Toolbox-Item">
                <div class="_icon"></div>
                <div class="_lbl">Process</div>
            </div>
            <div id="noah-webui-default-Print" class="btn-tb-action btn-tb-print noah-webui-Toolbox-Item nwHide">
                <div class="_icon"></div>
                <div class="_lbl">Print</div>
            </div>
            <div id="noah-webui-default-Export" class="btn-tb-action btn-tb-export noah-webui-Toolbox-Item nwHide">
                <div class="_icon"></div>
                <div class="_lbl">Export</div>
            </div>
        </div>
    </div>
    <div class="container-flex padding-10">
            <div class="row row-parent">
                <div class="col col-6 col-parent">
                    <div class="row with-label-lookup">
                        <div class="col col-4">
                            <label>Invoice Type</label>
                        </div>
                        <div id="lugInvoice" class="col col-8 col-parent lookups">
                            <div class="col col-4">
                                <input type="text" class="txtbox idval" />
                            </div>
                            <div class="col col-8">
                                <input type="text" class="txtbox descval" />
                            </div>
                        </div>
                    </div>
                    <div class="row with-label-lookup">
                         <div class="col col-4">
                             <label>Location With Accountable Forms <span class="nwRequiredField">*</span></label>
                         </div>
                         <div id="lugAccountableForms" class="col col-8 col-parent lookups">
                             <div class="col col-4">
                                 <input class="txtbox idval" type="text" />
                             </div>
                             <div class="col col-8">
                                 <input class="txtbox descval" type="text" />
                                 <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                             </div>
                         </div>
                     </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Phase/Tower<span class="nwRequiredField">*</span></label>
                        </div>
                        <div id="lugPhaseTower" class="col col-8 col-parent lookups">
                            <div class="col col-4">
                                <input class="txtbox idval" type="text" />
                            </div>
                            <div class="col col-8">
                                <input class="txtbox descval" type="text" />
                                <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                            </div>
                        </div>
                    </div>
                    <div class="row with-label-lookup">
                        <div class="col col-4">
                            <label>Project</label>
                        </div>
                        <div id="lugProject" class="col col-8 col-parent lookups">
                            <div class="col col-4">
                                <input type="text" class="txtbox idval" />
                            </div>
                            <div class="col col-8">
                                <input type="text"  class="txtbox descval"/>
                            </div>
                        </div>
                    </div>
                    <div class="row with-label-lookup">
                        <div class="col col-4">
                            <label>Utility<span class="nwRequiredField">*</span></label>
                        </div>
                        <div id="lugUtility" class="col col-8 col-parent lookups">
                            <div class="col col-4">
                                <input class="txtbox idval" type="text" />
                            </div>
                            <div class="col col-8">
                                <input class="txtbox descval" type="text" />
                                <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Bill Period From <span class="nwRequiredField">*</span></label>
                        </div>
                        <div class="col col-8">
                            <input id="txtBillPeriodFrom" class="nwDatePick" type="text" maxlength="12" placeholder="MM/DD/YYYY" />
                            <input id="txtLoadedPeriodFrom" class="nwDatePick nwHide" type="text" maxlength="12" placeholder="MM/DD/YYYY" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Bill Period To <span class="nwRequiredField">*</span></label>
                        </div>
                        <div class="col col-8">
                            <input id="txtBillPeriodTo" class="nwDatePick" type="text" maxlength="12" placeholder="MM/DD/YYYY" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Invoice Date<span class="nwRequiredField">*</span></label>
                        </div>
                        <div class="col col-8">
                            <input id="dtpBillingDate" class="nwDatePick" type="text" maxlength="12" placeholder="MM/DD/YYYY" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Total Utility Bill<span class="nwRequiredField" id="ubill">*</span></label>
                        </div>
                        <div class="col col-8">
                            <input id="txtTotalUtilityBill" class="idval  isNumber numC" nwdp="2" type="text" maxlength="12" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Total Billable Amount</label>
                        </div>
                        <div class="col col-8">
                            <input id="txtTotalBIllableAmount" class="idval  isNumber numC" nwdp="2" type="text" maxlength="80" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Adjustments</label>
                        </div>
                        <div class="col col-8">
                            <input id="txtAdjustments" class="idval  isNumber numC" nwdp="2" type="text"  maxlength="80" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Remarks</label>
                        </div>
                        <div class="col col-8">
                            <textarea id="txtRemarks" class="txtarea noresize" rows="3" maxlength="500"></textarea>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label></label>
                        </div>
                        <div class="col col-8">
                            <div id="btnUtilityRate" class="btn btn-default btn-default-orange fullwidth">Utility Rate</div>
                        </div>
                    </div>
                    <input id="txtTotalConsumption" type="text" maxlength="255" class="nwHide" />
                    <input id="txtUtilityRate" type="text" maxlength="255" class="nwHide" />
                    
                    <input id="txtTotalBillServiceProvider" type="text" maxlength="255" class="nwHide" />
                    <input id="txtTotalConsumptionServiceProvider" type="text" maxlength="255" class="nwHide" />
                    <input id="txtUtilityRateServiceProvider" type="text" maxlength="255" class="nwHide" />
                    <input id="txtTotalBillPropertyReading" type="text" maxlength="255" class="nwHide" />
                    <input id="txtTotalConsumptionPropertyReading" type="text" maxlength="255" class="nwHide" />
                    <input id="txtUtilityRatePropertyReading" type="text" maxlength="255" class="nwHide" />
                    <input id="txtTotalConsumptionVariance" type="text" maxlength="255" class="nwHide" />
                    <input id="txtUtilityRateVariance" type="text" maxlength="255" class="nwHide" />
                    <input id="txtTotalConsumptionPercent" type="text" maxlength="255" class="nwHide" />
                    <input id="txtUtilityRatePercent" type="text" maxlength="255" class="nwHide" />
                    <div><input id="txtEnableSeg" type="text" maxlength="255" class="nwHide" /></div>
                    <input id="isFixedMain" type="text" maxlength="255" class="nwHide" />

                </div>
                <div class="col col-6 col-parent">
                    <div class="row">
                        <div class="col col-4">
                            <label>Batch No.</label>
                        </div>
                        <div class="col col-8">
                            <input id="txtTransactionNo" class="idval" type="text" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Transaction Status</label>
                        </div>
                        <div class="col col-8">
                            <input id="txtStatus" class="idval" type="text"  />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Reason for Disapproval</label>
                        </div>
                        <div class="col col-8">
                            <input id="txtReasonforDisapproval" class="idval  isNumber numC" nwdp="2" type="text" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-4">
                            <label>Disapproval Remarks</label>
                        </div>
                        <div class="col col-8">
                            <input id="txtDisapprovalRemarks" class="idval  isNumber numC" nwdp="2" type="text" />
                        </div>
                    </div>
                    <div class="row with-label-lookup">
                        <div class="col col-4">
                            <label id="lblseg3">Cost Center<span class="nwRequiredField" id="reqseg3">*</span></label>
                        </div>
                        <div id="lugCostCenter" class="col col-8 col-parent lookups">
                            <div class="col col-4">
                                <input class="txtbox idval" type="text" />
                            </div>
                            <div class="col col-8">
                                <input class="txtbox descval" type="text" />
                                <div class="btn btn-sm-default btn-sm-default-lookup"></div>
                            </div>
                        </div>
                    </div>
                   
                    
                    
                    
                    
                    <div class="row">
                        <div class="col col-4">
                            <label></label>
                        </div>
                        <div class="col col-8">
                            <div id="btnDocumentAttachment" class="btn btn-default btn-default-orange fullwidth">Requirements Compliance</div>

                        </div>
                    </div>
                    
                </div>
             </div>

            <div class="row row-parent">
                <label><b>Utility Billing Details</b></label>
                <div class="col col-12 col-parent">
                    <div class="row">
                        <div id="nwGridMainCon" class="fullwidth">
                        </div>
                    </div>
                </div>
            </div>
    </div>
    <div class="nk-footer-c">
        <div class="nk-footer-info">
            <div class="nk-footer-create">
                <div class="nkf nk-f-createBy">
                    <svg xmlns="http://www.w3.org/2000/svg" width="22px" height="22px" class="svg-docFooter-icon" data-name="Layer 1" viewBox="0 0 22 22">
