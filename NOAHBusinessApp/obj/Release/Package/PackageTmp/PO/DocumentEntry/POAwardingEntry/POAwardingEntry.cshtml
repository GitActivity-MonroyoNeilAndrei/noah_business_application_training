                    <div class="col col-3">
                        <label class="lbltxt">Reason for Disapproval</label>
                    </div>

                    <div class="col col-5" id="lugRsnDisapprv">
                        <div class="col col-4 conval">
                            <input type="text" name="" class="txtbox idvallugRsnDisapprv lookupcodecss clrtxt" fdprocessedid="ak7ekj"/>
                        </div>
                        <div class="col col-8 conval">
                            <input type="text" name="" class="txtbox descvallugRsnDisapprv clrtxt" fdprocessedid="bn0t8f" />
                        </div>
                    </div>

                    <div class="col col-4">
                    </div>
                </div>

                @*Disapproval Remarks*@
                <div class="row">
                    <div class="col col-3">
                        <label class="lbltxt">Disapproval Remarks</label>
                    </div>
                    <div class="col col-5">
                        <textarea class="txtarea autoresize" rows="3" id="txtDisapprvRemarks"></textarea>
                    </div>
                    <div class="col col-4">
                    </div>
                </div>

                @*Value Date*@
                <div class="row nwHide">
                    <div class="col col-3">
                        <label class="lbltxt">Value Date<span class="nwRequiredField">*</span></label>
                    </div>
                    <div class="col col-5">
                        <input type="date" name="" class="txtbox txtbox-date" id="txtValueDate" />
                    </div>
                    <div class="col col-4">
                    </div>
                </div>

            </div>

            <div class="col col-2"></div>

            <div class="row padding-y-10"></div>

